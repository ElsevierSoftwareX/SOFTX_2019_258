PREFIX schema:    <http://schema.org/>
PREFIX rdfs:      <http://www.w3.org/2000/01/rdf-schema#>
PREFIX pc:        <http://purl.org/procurement/public-contracts#>
PREFIX gr:        <http://purl.org/goodrelations/v1#>
PREFIX owl:       <http://www.w3.org/2002/07/owl#>
PREFIX adms:      <http://www.w3.org/ns/adms#>
PREFIX c4n:       <http://vocab.deri.ie/c4n#>
PREFIX dcterms:   <http://purl.org/dc/terms/>
PREFIX foaf:      <http://xmlns.com/foaf/0.1/>
PREFIX loted:     <http://www.loted.eu/ontology#>
PREFIX payment:   <http://reference.data.gov.uk/def/payment#>
PREFIX qb:        <http://purl.org/linked-data/cube#>
PREFIX rdf:       <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX skos:      <http://www.w3.org/2004/02/skos/core#>
PREFIX vann:      <http://purl.org/vocab/vann/>
PREFIX xsd:       <http://www.w3.org/2001/XMLSchema#>
PREFIX conference: <http://conference#>
PREFIX cmt:       <http://cmt#>
PREFIX sigkdd:    <http://sigkdd#>
PREFIX crm:       <http://erlangen-crm.org/current/>
PREFIX jarql:     <http://jarql.com/>

CONSTRUCT {
    ?E82_Actor_Appellation0 rdf:label ?Attribution.
    ?E82_Actor_Appellation0 rdf:type crm:E82_Actor_Appellation.
    ?E82_Actor_Appellation0 rdfs:label ?Alpha_Sort.
    ?E82_Actor_Appellation0 rdf:type crm:E82_Actor_Appellation.
    ?E52_Time-Span0 crm:P82_at_some_time_within ?Begin_Date.
    ?E52_Time-Span0 rdf:type crm:E52_Time-Span.
    ?E52_Time-Span0 crm:P82_at_some_time_within ?End_Date.
    ?E52_Time-Span0 rdf:type crm:E52_Time-Span.
    ?E35_Title0 rdfs:label ?Title.
    ?E35_Title0 rdf:type crm:E35_Title.
    ?E52_Time-Span0 crm:P82_at_some_time_within ?Dated.
    ?E52_Time-Span0 rdf:type crm:E52_Time-Span.
    ?E55_Type0 rdfs:label ?Medium.
    ?E55_Type0 rdf:type crm:E55_Type.
    ?E54_Dimension0 crm:P3_has_note ?Dimensions.
    ?E54_Dimension0 rdf:type crm:E54_Dimension.
    ?E71_Man-Made_Thing0 crm:P67i_is_referred_to_by ?E35_Title0.
    ?E54_Dimension0 crm:P84i_was_maximum_duration_of ?E52_Time-Span0.
    ?E70_Thing0 crm:P101_had_as_general_use ?E55_Type0.
    ?E54_Dimension0 crm:P43i_is_dimension_of ?E70_Thing0.
    ?E55_Type0 crm:P130i_features_are_also_found_on ?E71_Man-Made_Thing0.
    ?E54_Dimension0 crm:P1_is_identified_by ?E82_Actor_Appellation0.
}
WHERE {
    OPTIONAL { ?root a jarql:Root . }
    OPTIONAL { jarql:root jarql:Attribution ?Attribution. }
    OPTIONAL { jarql:root jarql:Alpha_Sort ?Alpha_Sort. }
    OPTIONAL { jarql:root jarql:Begin_Date ?Begin_Date. }
    OPTIONAL { jarql:root jarql:End_Date ?End_Date. }
    OPTIONAL { jarql:root jarql:Title ?Title. }
    OPTIONAL { jarql:root jarql:Dated ?Dated. }
    OPTIONAL { jarql:root jarql:Medium ?Medium. }
    OPTIONAL { jarql:root jarql:Dimensions ?Dimensions. }
    BIND (URI(CONCAT('undefined',?Alpha_Sort)) as ?E82_Actor_Appellation0)
    BIND (URI(CONCAT('undefined',?Title)) as ?E35_Title0)
    BIND (URI(CONCAT('undefined',?Medium)) as ?E55_Type0)
    BIND (URI(CONCAT('undefined',?Title)) as ?E71_Man-Made_Thing0)
    BIND (URI(CONCAT('undefined',?Medium)) as ?E70_Thing0)
}
