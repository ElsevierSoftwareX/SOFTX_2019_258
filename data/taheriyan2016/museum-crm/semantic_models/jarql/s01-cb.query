PREFIX bmo:       <http://collection.britishmuseum.org/id/ontology/>
PREFIX jarql:     <http://jarql.com/>

CONSTRUCT {
    ?E82_Actor_Appellation1 rdf:label ?Attribution.
    ?E82_Actor_Appellation1 rdf:type crm:E82_Actor_Appellation.
    ?E82_Actor_Appellation1 rdfs:label ?Alpha_Sort.
    ?E82_Actor_Appellation1 rdf:type crm:E82_Actor_Appellation.
    ?E52_Time-Span2 crm:P82_at_some_time_within ?Begin_Date.
    ?E52_Time-Span2 rdf:type crm:E52_Time-Span.
    ?E52_Time-Span3 crm:P82_at_some_time_within ?End_Date.
    ?E52_Time-Span3 rdf:type crm:E52_Time-Span.
    ?E35_Title1 rdfs:label ?Title.
    ?E35_Title1 rdf:type crm:E35_Title.
    ?E52_Time-Span1 crm:P82_at_some_time_within ?Dated.
    ?E52_Time-Span1 rdf:type crm:E52_Time-Span.
    ?E55_Type1 rdfs:label ?Medium.
    ?E55_Type1 rdf:type crm:E55_Type.
    ?E54_Dimension1 crm:P3_has_note ?Dimensions.
    ?E54_Dimension1 rdf:type crm:E54_Dimension.
    ?E71_Man-Made_Thing1 crm:P67i_is_referred_to_by ?E35_Title1.
    ?E54_Dimension1 crm:P84i_was_maximum_duration_of ?E52_Time-Span1.
    ?E54_Dimension1 crm:P84i_was_maximum_duration_of ?E52_Time-Span2.
    ?E54_Dimension1 crm:P84i_was_maximum_duration_of ?E52_Time-Span3.
    ?E70_Thing1 crm:P101_had_as_general_use ?E55_Type1.
    ?E54_Dimension1 crm:P43i_is_dimension_of ?E70_Thing1.
    ?E55_Type1 crm:P130i_features_are_also_found_on ?E71_Man-Made_Thing1.
    ?E54_Dimension1 crm:P1_is_identified_by ?E82_Actor_Appellation1.
}
WHERE {
    ?root a jarql:Root.
    OPTIONAL { ?root jarql:Attribution ?Attribution. }
    OPTIONAL { ?root jarql:Alpha_Sort ?Alpha_Sort. }
    OPTIONAL { ?root jarql:Begin_Date ?Begin_Date. }
    OPTIONAL { ?root jarql:End_Date ?End_Date. }
    OPTIONAL { ?root jarql:Title ?Title. }
    OPTIONAL { ?root jarql:Dated ?Dated. }
    OPTIONAL { ?root jarql:Medium ?Medium. }
    OPTIONAL { ?root jarql:Dimensions ?Dimensions. }
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e82_actor_appellation/',?Alpha_Sort)) as ?E82_Actor_Appellation1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e35_title/',?Title)) as ?E35_Title1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e55_type/',?Medium)) as ?E55_Type1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e71_man-made_thing/',?Title)) as ?E71_Man-Made_Thing1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e70_thing/',?Medium)) as ?E70_Thing1)
}