PREFIX skos:      <http://www.w3.org/2004/02/skos/core#>
PREFIX vann:      <http://purl.org/vocab/vann/>
PREFIX xsd:       <http://www.w3.org/2001/XMLSchema#>
PREFIX conference: <http://conference#>
PREFIX cmt:       <http://cmt#>
PREFIX sigkdd:    <http://sigkdd#>
PREFIX crm:       <http://erlangen-crm.org/current/>
PREFIX id:        <http://collection.britishmuseum.org/id/>
PREFIX idThes:    <http://collection.britishmuseum.org/id/thesauri/>
PREFIX prov:      <http://www.w3.org/ns/prov#>
PREFIX bmo:       <http://collection.britishmuseum.org/id/ontology/>
PREFIX aac-ont:   <http://www.americanartcollaborative.org/ontology/>
PREFIX skos2:     <http://www.w3.org/2008/05/skos#>
PREFIX ElementsGr2: <http://rdvocab.info/ElementsGr2/>
PREFIX rdvocab-schema: <http://rdvocab.info/uri/schema/FRBRentitiesRDA/>
PREFIX edm:       <http://www.europeana.eu/schemas/edm/>
PREFIX schema-dig: <http://schema.dig.isi.edu/ontology/>
PREFIX geo:       <http://www.w3.org/2003/01/geo/wgs84_pos#>
PREFIX frbr:      <http://purl.org/vocab/frbr/core#>
PREFIX swap:      <http://www.w3.org/2000/10/swap/pim/contact#>
PREFIX cidoc-crm: <http://www.cidoc-crm.org/rdfs/cidoc-crm#>
PREFIX abc:       <http://metadata.net/harmony/abc#>
PREFIX DOLCE-Lite: <http://www.loa-cnr.it/ontologies/DOLCE-Lite.owl#>
PREFIX dcmitype:  <http://purl.org/dc/dcmitype/>
PREFIX msg0:      <http://web.resource.org/cc/>
PREFIX time-entry: <http://www.isi.edu/~pan/damltime/time-entry.owl#>
PREFIX wordnet:   <http://xmlns.com/wordnet/1.6/Work~2>
PREFIX saam-ont:  <http://americanart.si.edu/ontology/>
PREFIX ore:       <http://www.openarchives.org/ore/terms/>
PREFIX scharp:    <http://scharp.usc.isi.edu/ontology/>
PREFIX memex:     <http://dig.isi.edu/ontology/memex/>
PREFIX dc:        <http://purl.org/dc/elements/1.1/>
PREFIX jarql:     <http://jarql.com/>

CONSTRUCT {
    ?CulturalHeritageObject1 dcterms:title ?title.
    ?CulturalHeritageObject1 rdf:type aac-ont:CulturalHeritageObject.
    ?CulturalHeritageObject1 dcterms:provenance ?collection_name.
    ?CulturalHeritageObject1 rdf:type aac-ont:CulturalHeritageObject.
    ?Concept2 skos2:prefLabel ?subject_tags.
    ?Concept2 rdf:type skos2:Concept.
    ?CulturalHeritageObject1 dcterms:description ?title.
    ?CulturalHeritageObject1 rdf:type aac-ont:CulturalHeritageObject.
    ?WebResource1 rdfs:label ?image_URL.
    ?WebResource1 rdf:type edm:WebResource.
    ?Concept1 skos2:prefLabel ?technique_type.
    ?Concept1 rdf:type skos2:Concept.
    ?CulturalHeritageObject1 dcterms:created ?dateCreated.
    ?CulturalHeritageObject1 rdf:type aac-ont:CulturalHeritageObject.
    ?CulturalHeritageObject1 aac-ont:accessionNumber ?accession_id.
    ?CulturalHeritageObject1 rdf:type aac-ont:CulturalHeritageObject.
    ?CulturalHeritageObject1 dcterms:medium ?materials.
    ?CulturalHeritageObject1 rdf:type aac-ont:CulturalHeritageObject.
    ?CulturalHeritageObject1 dcterms:extent ?dimensions.
    ?CulturalHeritageObject1 rdf:type aac-ont:CulturalHeritageObject.
    ?Person1 ElementsGr2:nameOfThePerson ?artist.
    ?Person1 rdf:type aac-ont:Person.
    ?Person1 skos2:semanticRelation ?CulturalHeritageObject1.
    ?ProvidedCHO1 aac-ont:sitter ?Person1.
    ?Aggregation1 edm:hasView ?WebResource1.
    ?Aggregation1 edm:aggregatedCHO ?ProvidedCHO1.
    ?image_URL rdfs:subClassOf owl:Thing.
    ?ConceptScheme2 skos2:hasTopConcept ?Concept2.
    ?ConceptScheme2 rdfs:subClassOf owl:Thing.
    ?Concept1 skos2:topConceptOf ?ConceptScheme1.
}
WHERE {
    ?root a jarql:Root.
    OPTIONAL { ?root jarql:title ?title. }
    OPTIONAL { ?root jarql:collection_name ?collection_name. }
    OPTIONAL { ?root jarql:subject_tags ?subject_tags. }
    OPTIONAL { ?root jarql:title ?title. }
    OPTIONAL { ?root jarql:image_URL ?image_URL. }
    OPTIONAL { ?root jarql:technique_type ?technique_type. }
    OPTIONAL { ?root jarql:dateCreated ?dateCreated. }
    OPTIONAL { ?root jarql:accession_id ?accession_id. }
    OPTIONAL { ?root jarql:materials ?materials. }
    OPTIONAL { ?root jarql:dimensions ?dimensions. }
    OPTIONAL { ?root jarql:artist ?artist. }
    BIND (URI(CONCAT('http://www.americanartcollaborative.org/ontology//culturalheritageobject/',?title)) as ?CulturalHeritageObject1)
    BIND (URI(CONCAT('http://purl.org/vocab/frbr/core/concept/',?subject_tags)) as ?Concept2)
    BIND (URI(CONCAT('http://www.europeana.eu/schemas/edm//webresource/',?image_URL)) as ?WebResource1)
    BIND (URI(CONCAT('http://purl.org/vocab/frbr/core/concept/',?technique_type)) as ?Concept1)
    BIND (URI(CONCAT('http://purl.org/vocab/frbr/core/person/',?artist)) as ?Person1)
    BIND (URI(CONCAT('http://www.europeana.eu/schemas/edm//providedcho/',?artist)) as ?ProvidedCHO1)
    BIND (URI(CONCAT('http://www.openarchives.org/ore/terms//aggregation/',?image_URL)) as ?Aggregation1)
    BIND (URI(CONCAT('http://www.w3.org/2008/05/skos/conceptscheme/',?subject_tags)) as ?ConceptScheme2)
    BIND (URI(CONCAT('http://www.w3.org/2008/05/skos/conceptscheme/',?technique_type)) as ?ConceptScheme1)
}