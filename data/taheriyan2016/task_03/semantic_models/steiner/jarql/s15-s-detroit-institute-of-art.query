PREFIX msg0:      <http://web.resource.org/cc/>
PREFIX time-entry: <http://www.isi.edu/~pan/damltime/time-entry.owl#>
PREFIX wordnet:   <http://xmlns.com/wordnet/1.6/Work~2>
PREFIX saam-ont:  <http://americanart.si.edu/ontology/>
PREFIX ore:       <http://www.openarchives.org/ore/terms/>
PREFIX jarql:     <http://jarql.com/>

CONSTRUCT {
    ?CulturalHeritageObject1 dcterms:extent ?dimensions.
    ?CulturalHeritageObject1 rdf:type aac-ont:CulturalHeritageObject.
    ?CulturalHeritageObject1 dcterms:medium ?medium.
    ?CulturalHeritageObject1 rdf:type aac-ont:CulturalHeritageObject.
    ?CulturalHeritageObject1 dcterms:provenance ?provenance.
    ?CulturalHeritageObject1 rdf:type aac-ont:CulturalHeritageObject.
    ?Person1 ElementsGr2:nameOfThePerson ?name.
    ?Person1 rdf:type aac-ont:Person.
    ?CulturalHeritageObject1 dcterms:provenance ?credit.
    ?CulturalHeritageObject1 rdf:type aac-ont:CulturalHeritageObject.
    ?Person1 ElementsGr2:dateOfDeath ?death.
    ?Person1 rdf:type aac-ont:Person.
    ?Concept1 skos2:prefLabel ?classification.
    ?Concept1 rdf:type skos2:Concept.
    ?CulturalHeritageObject1 dcterms:title ?title.
    ?CulturalHeritageObject1 rdf:type aac-ont:CulturalHeritageObject.
    ?CulturalHeritageObject1 dcterms:created ?date.
    ?CulturalHeritageObject1 rdf:type aac-ont:CulturalHeritageObject.
    ?WebResource1 rdfs:label ?imageURL.
    ?WebResource1 rdf:type edm:WebResource.
    ?Person1 ElementsGr2:countryAssociatedWithThePerson ?nationality.
    ?Person1 rdf:type aac-ont:Person.
    ?Person1 ElementsGr2:dateOfBirth ?birth.
    ?Person1 rdf:type aac-ont:Person.
    ?CulturalHeritageObject1 aac-ont:accessionNumber ?accession.
    ?CulturalHeritageObject1 rdf:type aac-ont:CulturalHeritageObject.
    ?CulturalHeritageObject1 skos2:semanticRelation ?Person1.
    ?ProvidedCHO1 aac-ont:sitter ?Person1.
    ?Aggregation1 edm:hasView ?WebResource1.
    ?Aggregationundefined edm:aggregatedCHO ?ProvidedCHOundefined.
    ?undefinedundefined rdfs:subClassOf owl:Thing.
    ?ConceptScheme1 skos2:hasTopConcept ?Concept1.
    ?ConceptSchemeundefined rdfs:subClassOf owl:Thing.
}
WHERE {
    ?root a jarql:Root.
    OPTIONAL { ?root jarql:dimensions ?dimensions. }
    OPTIONAL { ?root jarql:medium ?medium. }
    OPTIONAL { ?root jarql:provenance ?provenance. }
    OPTIONAL { ?root jarql:name ?name. }
    OPTIONAL { ?root jarql:credit ?credit. }
    OPTIONAL { ?root jarql:death ?death. }
    OPTIONAL { ?root jarql:classification ?classification. }
    OPTIONAL { ?root jarql:title ?title. }
    OPTIONAL { ?root jarql:date ?date. }
    OPTIONAL { ?root jarql:imageURL ?imageURL. }
    OPTIONAL { ?root jarql:nationality ?nationality. }
    OPTIONAL { ?root jarql:birth ?birth. }
    OPTIONAL { ?root jarql:accession ?accession. }
    BIND (URI(CONCAT('http://www.americanartcollaborative.org/ontology//culturalheritageobject/',?dimensions)) as ?CulturalHeritageObject1)
    BIND (URI(CONCAT('http://purl.org/vocab/frbr/core/person/',?name)) as ?Person1)
    BIND (URI(CONCAT('http://purl.org/vocab/frbr/core/concept/',?classification)) as ?Concept1)
    BIND (URI(CONCAT('http://www.europeana.eu/schemas/edm//webresource/',?imageURL)) as ?WebResource1)
    BIND (URI(CONCAT('http://www.europeana.eu/schemas/edm//providedcho/',?name)) as ?ProvidedCHO1)
    BIND (URI(CONCAT('http://www.openarchives.org/ore/terms//aggregation/',?imageURL)) as ?Aggregation1)
    BIND (URI(CONCAT('http://www.w3.org/2008/05/skos/conceptscheme/',?classification)) as ?ConceptScheme1)
}