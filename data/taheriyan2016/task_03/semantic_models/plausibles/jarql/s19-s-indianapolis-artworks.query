    ?ConceptScheme1 skos2:hasTopConcept ?Concept1.
    ?Person2 foaf:workplaceHomepage ?Document2.
    ?Person1 foaf:workplaceHomepage ?Document1.
    ?Person2 foaf:workInfoHomepage ?Document2.
    ?Person1 foaf:workInfoHomepage ?Document1.
    ?Person2 foaf:schoolHomepage ?Document2.
    ?Person1 foaf:schoolHomepage ?Document1.
    ?Person2 foaf:publications ?Document2.
    ?Person1 foaf:publications ?Document1.
    ?Agent2 foaf:weblog ?Document2.
    ?Agent1 foaf:weblog ?Document1.
    ?Agent2 foaf:openid ?Document2.
    ?Agent1 foaf:openid ?Document1.
