PREFIX msg0:      <http://web.resource.org/cc/>
PREFIX time-entry: <http://www.isi.edu/~pan/damltime/time-entry.owl#>
PREFIX wordnet:   <http://xmlns.com/wordnet/1.6/Work~2>
PREFIX saam-ont:  <http://americanart.si.edu/ontology/>
PREFIX ore:       <http://www.openarchives.org/ore/terms/>
PREFIX scharp:    <http://scharp.usc.isi.edu/ontology/>
PREFIX memex:     <http://dig.isi.edu/ontology/memex/>
PREFIX dc:        <http://purl.org/dc/elements/1.1/>
PREFIX jarql:     <http://jarql.com/>

CONSTRUCT {
    ?CulturalHeritageObject1 dcterms:title ?Object Title.
    ?CulturalHeritageObject1 rdf:type aac-ont:CulturalHeritageObject.
    ?CulturalHeritageObject1 dcterms:created ?Object Earliest Date.
    ?CulturalHeritageObject1 rdf:type aac-ont:CulturalHeritageObject.
    ?Concept1 skos2:prefLabel ?Object Work Type.
    ?Concept1 rdf:type skos2:Concept.
    ?Person1 ElementsGr2:nameOfThePerson ?Artist Name.
    ?Person1 rdf:type aac-ont:Person.
    ?CulturalHeritageObject1 dcterms:provenance ?Object Rights.
    ?CulturalHeritageObject1 rdf:type aac-ont:CulturalHeritageObject.
    ?CulturalHeritageObject1 dcterms:rightsHolder ?Object Link Source.
    ?CulturalHeritageObject1 rdf:type aac-ont:CulturalHeritageObject.
    ?CulturalHeritageObject1 dcterms:extent ?dimensions.
    ?CulturalHeritageObject1 rdf:type aac-ont:CulturalHeritageObject.
    ?CulturalHeritageObject1 dcterms:medium ?Object Facet Value 1.
    ?CulturalHeritageObject1 rdf:type aac-ont:CulturalHeritageObject.
    ?Person1 ElementsGr2:dateOfBirth ?Artist Birth Date.
    ?Person1 rdf:type aac-ont:Person.
    ?Person1 ElementsGr2:dateOfDeath ?Artist Death Date.
    ?Person1 rdf:type aac-ont:Person.
    ?CulturalHeritageObject1 aac-ont:accessionNumber ?Object ID.
    ?CulturalHeritageObject1 rdf:type aac-ont:CulturalHeritageObject.
    ?Person1 ElementsGr2:countryAssociatedWithThePerson ?Artist Nationality.
    ?Person1 rdf:type aac-ont:Person.
    ?CulturalHeritageObject1 aac-ont:sitter ?Person1.
    ?Concept1 skos2:mappingRelation ?Concept1.
    ?Concept1 skos2:semanticRelation ?Concept1.
    ?Concept1 skos2:topConceptOf ?ConceptScheme1.
    ?ConceptScheme1 skos2:hasTopConcept ?Concept1.
    ?Person1 ElementsGr2:placeOfBirth ?Place1.
    ?Person1 ElementsGr2:placeOfDeath ?Place1.
    ?ProvidedCHO1 aac-ont:sitter ?Person1.
    ?CulturalHeritageObject1 skos2:mappingRelation ?Person1.
    ?CulturalHeritageObject1 skos2:semanticRelation ?Person1.
    ?CulturalHeritageObject1 skos2:mappingRelation ?Place1.
    ?CulturalHeritageObject1 skos2:semanticRelation ?Place1.
    ?Person1 skos2:mappingRelation ?CulturalHeritageObject1.
    ?Person1 skos2:semanticRelation ?CulturalHeritageObject1.
    ?Person1 skos2:mappingRelation ?Place1.
    ?Person1 skos2:semanticRelation ?Place1.
    ?Place1 skos2:mappingRelation ?CulturalHeritageObject1.
    ?Place1 skos2:semanticRelation ?CulturalHeritageObject1.
    ?Place1 skos2:mappingRelation ?Person1.
    ?Place1 skos2:semanticRelation ?Person1.
    ?ProvidedCHO1 rdfs:subClassOf owl:Thing.
    ?ConceptScheme1 rdfs:subClassOf owl:Thing.
}
WHERE {
    ?root a jarql:Root.
    OPTIONAL { ?root jarql:Object Title ?Object Title. }
    OPTIONAL { ?root jarql:Object Earliest Date ?Object Earliest Date. }
    OPTIONAL { ?root jarql:Object Work Type ?Object Work Type. }
    OPTIONAL { ?root jarql:Artist Name ?Artist Name. }
    OPTIONAL { ?root jarql:Object Rights ?Object Rights. }
    OPTIONAL { ?root jarql:Object Link Source ?Object Link Source. }
    OPTIONAL { ?root jarql:dimensions ?dimensions. }
    OPTIONAL { ?root jarql:Object Facet Value 1 ?Object Facet Value 1. }
    OPTIONAL { ?root jarql:Artist Birth Date ?Artist Birth Date. }
    OPTIONAL { ?root jarql:Artist Death Date ?Artist Death Date. }
    OPTIONAL { ?root jarql:Object ID ?Object ID. }
    OPTIONAL { ?root jarql:Artist Nationality ?Artist Nationality. }
    BIND (URI(CONCAT('http://www.americanartcollaborative.org/ontology//culturalheritageobject/',?Object Title)) as ?CulturalHeritageObject1)
    BIND (URI(CONCAT('http://purl.org/vocab/frbr/core/concept/',?Object Work Type)) as ?Concept1)
    BIND (URI(CONCAT('http://purl.org/vocab/frbr/core/person/',?Artist Name)) as ?Person1)
    BIND (URI(CONCAT('http://www.w3.org/2008/05/skos/conceptscheme/',?Object Work Type)) as ?ConceptScheme1)
    BIND (URI(CONCAT('http://purl.org/vocab/frbr/core/place/',?Artist Name)) as ?Place1)
    BIND (URI(CONCAT('http://www.europeana.eu/schemas/edm//providedcho/',?Artist Name)) as ?ProvidedCHO1)
}