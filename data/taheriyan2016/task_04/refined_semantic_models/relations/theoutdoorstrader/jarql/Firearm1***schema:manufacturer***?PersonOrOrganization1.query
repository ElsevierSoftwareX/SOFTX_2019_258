PREFIX jarql:     <http://jarql.com/>

CONSTRUCT {
    ?Firearm1 schema:manufacturer ?PersonOrOrganization1.
}
WHERE {
    ?root a jarql:Root.
    OPTIONAL { ?root jarql:Accessories0378 ?Accessories0378. }
    OPTIONAL { ?root jarql:Name0846 ?Name0846. }
    OPTIONAL { ?root jarql:started0628 ?started0628. }
    OPTIONAL { ?root jarql:Messages0801 ?Messages0801. }
    OPTIONAL { ?root jarql:trade1645 ?trade1645. }
    OPTIONAL { ?root jarql:file_name ?file_name. }
    OPTIONAL { ?root jarql:Description0868 ?Description0868. }
    OPTIONAL { ?root jarql:FT0613 ?FT0613. }
    OPTIONAL { ?root jarql:0029 ?0029. }
    OPTIONAL { ?root jarql:Price0855 ?Price0855. }
    OPTIONAL { ?root jarql:Location0814 ?Location0814. }
    OPTIONAL { ?root jarql:For0031 ?For0031. }
    BIND (URI(CONCAT('http://schema.dig.isi.edu/ontology//firearm/',?Accessories0378)) as ?Firearm1)
    BIND (URI(CONCAT('http://schema.dig.isi.edu/ontology//personororganization/',?started0628)) as ?PersonOrOrganization1)
    BIND (URI(CONCAT('http://subclass_of_thing/',?file_name)) as ?CreativeWork1)
    BIND (URI(CONCAT('http://subclass_of_thing/',?FT0613)) as ?Offer1)
    BIND (URI(CONCAT('http://dig.isi.edu/ontology/memex//place/',?Location0814)) as ?Place1)
    BIND (URI(CONCAT('http://subclass_of_thing/',?For0031)) as ?PostalAddress1)
}