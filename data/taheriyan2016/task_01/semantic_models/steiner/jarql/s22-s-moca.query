CONSTRUCT {
    ?E12_Production1 crm:P3_has_note ?Process.
    ?E12_Production1 rdf:type crm:E12_Production.
    ?E21_Person1 rdfs:label ?person_uri.
    ?E21_Person1 rdf:type crm:E21_Person.
    ?E22_Man-Made_Object1 rdfs:label ?object_uri.
    ?E22_Man-Made_Object1 rdf:type crm:E22_Man-Made_Object.
    ?E35_Title1 rdfs:label ?Artwork_Name.
    ?E35_Title1 rdf:type crm:E35_Title.
    ?E38_Image1 rdfs:label ?image.
    ?E38_Image1 rdf:type crm:E38_Image.
    ?E52_Time-Span1 crm:P82_at_some_time_within ?Artwork_Year.
    ?E52_Time-Span1 rdf:type crm:E52_Time-Span.
    ?E54_Dimension1 crm:P3_has_note ?Dimensions.
    ?E54_Dimension1 rdf:type crm:E54_Dimension.
    ?E82_Actor_Appellation1 rdfs:label ?person_appellation_uri.
    ?E82_Actor_Appellation1 rdf:type crm:E82_Actor_Appellation.
    ?E82_Actor_Appellation1 rdfs:label ?name.
    ?E82_Actor_Appellation1 rdf:type crm:E82_Actor_Appellation.
    ?E8_Acquisition1 crm:P3_has_note ?Provenance.
    ?E8_Acquisition1 rdf:type crm:E8_Acquisition.
    ?E8_Acquisition1 crm:P20i_was_purpose_of ?E12_Production1.
    ?E8_Acquisition1 crm:P24_transferred_title_of ?E18_Physical_Thing1.
    ?E21_Person1 id:ontology/PX_pupil_of ?E39_Actor1.
    ?E8_Acquisition1 crm:P19_was_intended_use_of ?E22_Man-Made_Object1.
    ?E82_Actor_Appellation1 crm:P106i_forms_part_of ?E35_Title1.
    ?E8_Acquisition1 crm:P16_used_specific_object ?E38_Image1.
    ?E18_Physical_Thing1 crm:P51_has_former_or_current_owner ?E39_Actor1.
    ?E82_Actor_Appellation1 crm:P1i_identifies ?E39_Actor1.
    ?E54_Dimension1 crm:P84i_was_maximum_duration_of ?E52_Time-Span1.
    ?E8_Acquisition1 crm:P17_was_motivated_by ?E54_Dimension1.
}
WHERE {
    ?root a jarql:Root.
    OPTIONAL { ?root jarql:Process ?Process. }
    OPTIONAL { ?root jarql:person_uri ?person_uri. }
    OPTIONAL { ?root jarql:object_uri ?object_uri. }
    OPTIONAL { ?root jarql:Artwork_Name ?Artwork_Name. }
    OPTIONAL { ?root jarql:image ?image. }
    OPTIONAL { ?root jarql:Artwork_Year ?Artwork_Year. }
    OPTIONAL { ?root jarql:Dimensions ?Dimensions. }
    OPTIONAL { ?root jarql:person_appellation_uri ?person_appellation_uri. }
    OPTIONAL { ?root jarql:name ?name. }
    OPTIONAL { ?root jarql:Provenance ?Provenance. }
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e21_person/',?person_uri)) as ?E21_Person1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e22_man-made_object/',?object_uri)) as ?E22_Man-Made_Object1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e38_image/',?image)) as ?E38_Image1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e82_actor_appellation/',?person_appellation_uri)) as ?E82_Actor_Appellation1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e18_physical_thing/',?Provenance)) as ?E18_Physical_Thing1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e39_actor/',?person_uri)) as ?E39_Actor1)
}