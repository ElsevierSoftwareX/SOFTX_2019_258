WHERE {
    ?root a jarql:Root.
    OPTIONAL { ?root jarql:Maker_URI ?Maker_URI. }
    OPTIONAL { ?root jarql:Description ?Description. }
    OPTIONAL { ?root jarql:Object_URI ?Object_URI. }
    OPTIONAL { ?root jarql:Collector Information ?Collector Information. }
    OPTIONAL { ?root jarql:Title ?Title. }
    OPTIONAL { ?root jarql:Owner ?Owner. }
    OPTIONAL { ?root jarql:Place Made ?Place Made. }
    OPTIONAL { ?root jarql:Maker Birth Date ?Maker Birth Date. }
    OPTIONAL { ?root jarql:Maker Death Date ?Maker Death Date. }
    OPTIONAL { ?root jarql:Home Location URI ?Home Location URI. }
    OPTIONAL { ?root jarql:Measurements ?Measurements. }
    OPTIONAL { ?root jarql:Category_URI ?Category_URI. }
    OPTIONAL { ?root jarql:Category ?Category. }
    OPTIONAL { ?root jarql:Materials_URI ?Materials_URI. }
    OPTIONAL { ?root jarql:Materials ?Materials. }
    OPTIONAL { ?root jarql:Collector_Information_typeURI ?Collector_Information_typeURI. }
    OPTIONAL { ?root jarql:Nationality_URI ?Nationality_URI. }
    OPTIONAL { ?root jarql:Nationality ?Nationality. }
    OPTIONAL { ?root jarql:Maker_Appellation_URI ?Maker_Appellation_URI. }
    OPTIONAL { ?root jarql:Maker ?Maker. }
    OPTIONAL { ?root jarql:Credit Line ?Credit Line. }
    OPTIONAL { ?root jarql:How Acquired? ?How Acquired?. }
    OPTIONAL { ?root jarql:Acquisition_URI ?Acquisition_URI. }
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e21_person/',?Maker_URI)) as ?E21_Person1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e22_man-made_object/',?Object_URI)) as ?E22_Man-Made_Object1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e53_place/',?Home Location URI)) as ?E53_Place2)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e55_type/',?Category_URI)) as ?E55_Type1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e55_type/',?Materials_URI)) as ?E55_Type2)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e55_type/',?Collector_Information_typeURI)) as ?E55_Type3)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e74_group/',?Nationality_URI)) as ?E74_Group1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e82_actor_appellation/',?Maker_Appellation_URI)) as ?E82_Actor_Appellation1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e8_acquisition/',?Acquisition_URI)) as ?E8_Acquisition1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e18_physical_thing/',?Credit Line)) as ?E18_Physical_Thing1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e39_actor/',?Maker_URI)) as ?E39_Actor1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e71_man-made_thing/',?Title)) as ?E71_Man-Made_Thing1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e18_physical_thing/',?Home Location URI)) as ?E18_Physical_Thing2)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e39_actor/',?Home Location URI)) as ?E39_Actor2)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e70_thing/',?Measurements)) as ?E70_Thing1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e70_thing/',?Collector_Information_typeURI)) as ?E70_Thing3)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e71_man-made_thing/',?Collector_Information_typeURI)) as ?E71_Man-Made_Thing3)
}