    ?E8_Acquisition1 rdf:type crm:E8_Acquisition.
    ?E8_Acquisition1 rdfs:label ?Acquisition_URI.
    ?E8_Acquisition1 rdf:type crm:E8_Acquisition.
    ?E8_Acquisition1 crm:P12_occurred_in_the_presence_of ?E18_Physical_Thing1.
    ?E21_Person1 id:ontology/PX_pupil_of ?E39_Actor1.
    ?E8_Acquisition1 crm:P62i_is_depicted_by ?E22_Man-Made_Object1.
    ?E8_Acquisition1 crm:P67i_is_referred_to_by ?E33_Linguistic_Object2.
    ?E71_Man-Made_Thing1 crm:P67i_is_referred_to_by ?E35_Title1.
    ?E82_Actor_Appellation1 crm:P1i_identifies ?E39_Actor1.
    ?E8_Acquisition1 crm:P12_occurred_in_the_presence_of ?E40_Legal_Body1.
    ?E8_Acquisition1 crm:P1_is_identified_by ?E48_Place_Name1.
    ?E54_Dimension1 crm:P84i_was_maximum_duration_of ?E52_Time-Span1.
    ?E54_Dimension1 crm:P84i_was_maximum_duration_of ?E52_Time-Span2.
    ?E18_Physical_Thing2 crm:P59_has_section ?E53_Place2.
    ?E39_Actor2 crm:P74_has_current_or_former_residence ?E53_Place2.
    ?E70_Thing1 crm:P43_has_dimension ?E54_Dimension1.
    ?E55_Type3 crm:P127i_has_narrower_term ?E55_Type1.
    ?E55_Type3 crm:P127i_has_narrower_term ?E55_Type2.
    ?E8_Acquisition1 crm:P12_occurred_in_the_presence_of ?E55_Type3.
    ?E55_Type3 crm:P101i_was_use_of ?E70_Thing3.
    ?E55_Type3 crm:P130i_features_are_also_found_on ?E71_Man-Made_Thing3.
