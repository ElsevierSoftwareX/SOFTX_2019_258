PREFIX schema:    <http://schema.org/>
PREFIX rdfs:      <http://www.w3.org/2000/01/rdf-schema#>
PREFIX pc:        <http://purl.org/procurement/public-contracts#>
PREFIX gr:        <http://purl.org/goodrelations/v1#>
PREFIX owl:       <http://www.w3.org/2002/07/owl#>
PREFIX adms:      <http://www.w3.org/ns/adms#>
PREFIX c4n:       <http://vocab.deri.ie/c4n#>
PREFIX dcterms:   <http://purl.org/dc/terms/>
PREFIX foaf:      <http://xmlns.com/foaf/0.1/>
PREFIX loted:     <http://www.loted.eu/ontology#>
PREFIX payment:   <http://reference.data.gov.uk/def/payment#>
PREFIX qb:        <http://purl.org/linked-data/cube#>
PREFIX rdf:       <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX skos:      <http://www.w3.org/2004/02/skos/core#>
PREFIX vann:      <http://purl.org/vocab/vann/>
PREFIX xsd:       <http://www.w3.org/2001/XMLSchema#>
PREFIX conference: <http://conference#>
PREFIX cmt:       <http://cmt#>
PREFIX sigkdd:    <http://sigkdd#>
PREFIX crm:       <http://erlangen-crm.org/current/>
PREFIX id:        <http://collection.britishmuseum.org/id/>
PREFIX idThes:    <http://collection.britishmuseum.org/id/thesauri/>
PREFIX prov:      <http://www.w3.org/ns/prov#>
PREFIX bmo:       <http://collection.britishmuseum.org/id/ontology/>
PREFIX jarql:     <http://jarql.com/>

CONSTRUCT {
    ?E33_Linguistic_Object1 crm:P3_has_note ?birth_data_and_place.
    ?E33_Linguistic_Object1 rdf:type crm:E33_Linguistic_Object.
    ?E33_Linguistic_Object2 crm:P3_has_note ?long_biography.
    ?E33_Linguistic_Object2 rdf:type crm:E33_Linguistic_Object.
    ?E33_Linguistic_Object3 crm:P3_has_note ?occupation.
    ?E33_Linguistic_Object3 rdf:type crm:E33_Linguistic_Object.
    ?E55_Type1 rdf:label ?biography_type_uri.
    ?E55_Type1 rdf:type crm:E55_Type.
    ?E55_Type2 rdf:label ?occupation_type_uri.
    ?E55_Type2 rdf:type crm:E55_Type.
    ?E74_Group1 rdf:label ?nationality_uri.
    ?E74_Group1 rdf:type crm:E74_Group.
    ?E74_Group2 rdfs:label ?nationality.
    ?E74_Group2 rdf:type crm:E74_Group.
    ?E82_Actor_Appellation1 rdfs:label ?artist_name.
    ?E82_Actor_Appellation1 rdf:type crm:E82_Actor_Appellation.
    ?E33_Linguistic_Object3 crm:P73i_is_translation_of ?E33_Linguistic_Object1.
    ?E33_Linguistic_Object3 crm:P73i_is_translation_of ?E33_Linguistic_Object2.
    ?E82_Actor_Appellation1 crm:P67i_is_referred_to_by ?E33_Linguistic_Object3.
    ?E82_Actor_Appellation1 crm:P1i_identifies ?E39_Actor1.
    ?E55_Type2 crm:P127i_has_narrower_term ?E55_Type1.
    ?E82_Actor_Appellation1 crm:P130i_features_are_also_found_on ?E55_Type2.
    ?E39_Actor1 id:ontology/PX_profession ?E74_Group1.
    ?E39_Actor2 id:ontology/PX_profession ?E74_Group2.
}
WHERE {
    ?root a jarql:Root.
    OPTIONAL { ?root jarql:birth_data_and_place ?birth_data_and_place. }
    OPTIONAL { ?root jarql:long_biography ?long_biography. }
    OPTIONAL { ?root jarql:occupation ?occupation. }
    OPTIONAL { ?root jarql:biography_type_uri ?biography_type_uri. }
    OPTIONAL { ?root jarql:occupation_type_uri ?occupation_type_uri. }
    OPTIONAL { ?root jarql:nationality_uri ?nationality_uri. }
    OPTIONAL { ?root jarql:nationality ?nationality. }
    OPTIONAL { ?root jarql:artist_name ?artist_name. }
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e55_type/',?biography_type_uri)) as ?E55_Type1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e55_type/',?occupation_type_uri)) as ?E55_Type2)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e74_group/',?nationality_uri)) as ?E74_Group1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e39_actor/',?artist_name)) as ?E39_Actor1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e39_actor/',?nationality)) as ?E39_Actor2)
}