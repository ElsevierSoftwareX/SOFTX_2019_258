PREFIX id:        <http://collection.britishmuseum.org/id/>
PREFIX idThes:    <http://collection.britishmuseum.org/id/thesauri/>
PREFIX prov:      <http://www.w3.org/ns/prov#>
PREFIX bmo:       <http://collection.britishmuseum.org/id/ontology/>
PREFIX jarql:     <http://jarql.com/>

CONSTRUCT {
    ?E21_Person1 crm:P3_has_note ?Artwork_in_Focus.
    ?E21_Person1 rdf:type crm:E21_Person.
    ?E21_Person2 rdfs:label ?Artist_URI.
    ?E21_Person2 rdf:type crm:E21_Person.
    ?E22_Man-Made_Object1 crm:P3_has_note ?Artwork_Details.
    ?E22_Man-Made_Object1 rdf:type crm:E22_Man-Made_Object.
    ?E22_Man-Made_Object2 rdfs:label ?Object_URI.
    ?E22_Man-Made_Object2 rdf:type crm:E22_Man-Made_Object.
    ?E35_Title1 rdfs:label ?Title.
    ?E35_Title1 rdf:type crm:E35_Title.
    ?E38_Image1 rdfs:label ?Image_URL.
    ?E38_Image1 rdf:type crm:E38_Image.
    ?E48_Place_Name1 rdfs:label ?Residence.
    ?E48_Place_Name1 rdf:type crm:E48_Place_Name.
    ?E52_Time-Span1 crm:P82_at_some_time_within ?Production_Year.
    ?E52_Time-Span1 rdf:type crm:E52_Time-Span.
    ?E52_Time-Span2 crm:P82_at_some_time_within ?birth_date.
    ?E52_Time-Span2 rdf:type crm:E52_Time-Span.
    ?E52_Time-Span3 crm:P82_at_some_time_within ?death_date.
    ?E52_Time-Span3 rdf:type crm:E52_Time-Span.
    ?E55_Type1 rdfs:label ?Classification_URI.
    ?E55_Type1 rdf:type crm:E55_Type.
    ?E55_Type2 rdfs:label ?Classification.
    ?E55_Type2 rdf:type crm:E55_Type.
    ?E82_Actor_Appellation1 rdfs:label ?Artist_Appellation_URI.
    ?E82_Actor_Appellation1 rdf:type crm:E82_Actor_Appellation.
    ?E82_Actor_Appellation2 rdfs:label ?Artist_Name.
    ?E82_Actor_Appellation2 rdf:type crm:E82_Actor_Appellation.
    ?E21_Person1 id:ontology/PX_pupil_of ?E39_Actor1.
    ?E21_Person2 id:ontology/PX_pupil_of ?E39_Actor2.
    ?E82_Actor_Appellation2 crm:P62i_is_depicted_by ?E22_Man-Made_Object1.
    ?E82_Actor_Appellation2 crm:P62i_is_depicted_by ?E22_Man-Made_Object2.
    ?E71_Man-Made_Thing1 crm:P67i_is_referred_to_by ?E35_Title1.
    ?E82_Actor_Appellation2 crm:P67i_is_referred_to_by ?E38_Image1.
    ?E82_Actor_Appellation2 crm:P1i_identifies ?E39_Actor2.
    ?E82_Actor_Appellation2 crm:P1_is_identified_by ?E48_Place_Name1.
    ?E52_Time-Span3 crm:P86i_contains ?E52_Time-Span1.
    ?E52_Time-Span3 crm:P86i_contains ?E52_Time-Span2.
    ?E54_Dimension3 crm:P84i_was_maximum_duration_of ?E52_Time-Span3.
    ?E70_Thingundefined crm:P43_has_dimension ?E54_Dimensionundefined.
    ?E55_Type2 crm:P127i_has_narrower_term ?E55_Type1.
    ?E82_Actor_Appellation2 crm:P130i_features_are_also_found_on ?E55_Type2.
    ?E55_Type2 crm:P101i_was_use_of ?E70_Thing2.
    ?E55_Type2 crm:P130i_features_are_also_found_on ?E71_Man-Made_Thing2.
    ?E39_Actor1 crm:P1_is_identified_by ?E82_Actor_Appellation1.
}
WHERE {
    ?root a jarql:Root.
    OPTIONAL { ?root jarql:Artwork_in_Focus ?Artwork_in_Focus. }
    OPTIONAL { ?root jarql:Artist_URI ?Artist_URI. }
    OPTIONAL { ?root jarql:Artwork_Details ?Artwork_Details. }
    OPTIONAL { ?root jarql:Object_URI ?Object_URI. }
    OPTIONAL { ?root jarql:Title ?Title. }
    OPTIONAL { ?root jarql:Image_URL ?Image_URL. }
    OPTIONAL { ?root jarql:Residence ?Residence. }
    OPTIONAL { ?root jarql:Production_Year ?Production_Year. }
    OPTIONAL { ?root jarql:birth_date ?birth_date. }
    OPTIONAL { ?root jarql:death_date ?death_date. }
    OPTIONAL { ?root jarql:Classification_URI ?Classification_URI. }
    OPTIONAL { ?root jarql:Classification ?Classification. }
    OPTIONAL { ?root jarql:Artist_Appellation_URI ?Artist_Appellation_URI. }
    OPTIONAL { ?root jarql:Artist_Name ?Artist_Name. }
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e21_person/',?Artist_URI)) as ?E21_Person2)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e22_man-made_object/',?Object_URI)) as ?E22_Man-Made_Object2)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e38_image/',?Image_URL)) as ?E38_Image1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e55_type/',?Classification_URI)) as ?E55_Type1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e82_actor_appellation/',?Artist_Appellation_URI)) as ?E82_Actor_Appellation1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e39_actor/',?Artwork_in_Focus)) as ?E39_Actor1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e39_actor/',?Artist_URI)) as ?E39_Actor2)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e71_man-made_thing/',?Title)) as ?E71_Man-Made_Thing1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e54_dimension/',?death_date)) as ?E54_Dimension3)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e70_thing/',?Classification)) as ?E70_Thing2)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e71_man-made_thing/',?Classification)) as ?E71_Man-Made_Thing2)
}