PREFIX bmo:       <http://collection.britishmuseum.org/id/ontology/>
PREFIX jarql:     <http://jarql.com/>

CONSTRUCT {
    ?E21_Person1 rdfs:label ?item__artist_uri.
    ?E21_Person1 rdf:type crm:E21_Person.
    ?E22_Man-Made_Object1 crm:P3_has_note ?item__description.
    ?E22_Man-Made_Object1 rdf:type crm:E22_Man-Made_Object.
    ?E35_Title1 rdfs:label ?item__title.
    ?E35_Title1 rdf:type crm:E35_Title.
    ?E38_Image1 rdfs:label ?item__image.
    ?E38_Image1 rdf:type crm:E38_Image.
    ?E52_Time-Span1 crm:P82_at_some_time_within ?item__year.
    ?E52_Time-Span1 rdf:type crm:E52_Time-Span.
    ?E55_Type1 rdfs:label ?item__technique_uri.
    ?E55_Type1 rdf:type crm:E55_Type.
    ?E55_Type2 rdfs:label ?item__technique.
    ?E55_Type2 rdf:type crm:E55_Type.
    ?E78_Collection1 rdfs:label ?item__name.
    ?E78_Collection1 rdf:type crm:E78_Collection.
    ?E82_Actor_Appellation1 rdfs:label ?item__artist_appellation_uri.
    ?E82_Actor_Appellation1 rdf:type crm:E82_Actor_Appellation.
    ?E82_Actor_Appellation2 rdfs:label ?item__artist.
    ?E82_Actor_Appellation2 rdf:type crm:E82_Actor_Appellation.
    ?E21_Person1 id:ontology/PX_pupil_of ?E39_Actor1.
    ?E82_Actor_Appellation2 crm:P62i_is_depicted_by ?E22_Man-Made_Object1.
    ?E71_Man-Made_Thing1 crm:P67i_is_referred_to_by ?E35_Title1.
    ?E82_Actor_Appellation2 crm:P67i_is_referred_to_by ?E38_Image1.
    ?E82_Actor_Appellation2 crm:P1i_identifies ?E39_Actor2.
    ?E54_Dimension1 crm:P84i_was_maximum_duration_of ?E52_Time-Span1.
    ?E70_Thingundefined crm:P43_has_dimension ?E54_Dimensionundefined.
    ?E55_Type2 crm:P127i_has_narrower_term ?E55_Type1.
    ?E82_Actor_Appellation2 crm:P130i_features_are_also_found_on ?E55_Type2.
    ?E55_Type2 crm:P101i_was_use_of ?E70_Thing2.
    ?E55_Type2 crm:P130i_features_are_also_found_on ?E71_Man-Made_Thing2.
    ?E39_Actor1 crm:P62i_is_depicted_by ?E78_Collection1.
    ?E39_Actor1 crm:P1_is_identified_by ?E82_Actor_Appellation1.
}
WHERE {
    ?root a jarql:Root.
    OPTIONAL { ?root jarql:item ?item. }
    OPTIONAL { ?item jarql:artist_uri ?item__artist_uri. }
    OPTIONAL { ?root jarql:item ?item. }
    OPTIONAL { ?item jarql:description ?item__description. }
    OPTIONAL { ?root jarql:item ?item. }
    OPTIONAL { ?item jarql:title ?item__title. }
    OPTIONAL { ?root jarql:item ?item. }
    OPTIONAL { ?item jarql:image ?item__image. }
    OPTIONAL { ?root jarql:item ?item. }
    OPTIONAL { ?item jarql:year ?item__year. }
    OPTIONAL { ?root jarql:item ?item. }
    OPTIONAL { ?item jarql:technique_uri ?item__technique_uri. }
    OPTIONAL { ?root jarql:item ?item. }
    OPTIONAL { ?item jarql:technique ?item__technique. }
    OPTIONAL { ?root jarql:item ?item. }
    OPTIONAL { ?item jarql:name ?item__name. }
    OPTIONAL { ?root jarql:item ?item. }
    OPTIONAL { ?item jarql:artist_appellation_uri ?item__artist_appellation_uri. }
    OPTIONAL { ?root jarql:item ?item. }
    OPTIONAL { ?item jarql:artist ?item__artist. }
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e21_person/',?item__artist_uri)) as ?E21_Person1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e38_image/',?item__image)) as ?E38_Image1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e55_type/',?item__technique_uri)) as ?E55_Type1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e82_actor_appellation/',?item__artist_appellation_uri)) as ?E82_Actor_Appellation1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e39_actor/',?item__artist_uri)) as ?E39_Actor1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e71_man-made_thing/',?item__title)) as ?E71_Man-Made_Thing1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e39_actor/',?item__artist)) as ?E39_Actor2)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e54_dimension/',?item__year)) as ?E54_Dimension1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e70_thing/',?item__technique)) as ?E70_Thing2)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e71_man-made_thing/',?item__technique)) as ?E71_Man-Made_Thing2)
}