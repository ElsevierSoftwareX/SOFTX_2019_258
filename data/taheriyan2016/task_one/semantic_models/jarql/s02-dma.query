PREFIX schema:    <http://schema.org/>
PREFIX rdfs:      <http://www.w3.org/2000/01/rdf-schema#>
PREFIX pc:        <http://purl.org/procurement/public-contracts#>
PREFIX gr:        <http://purl.org/goodrelations/v1#>
PREFIX owl:       <http://www.w3.org/2002/07/owl#>
PREFIX adms:      <http://www.w3.org/ns/adms#>
PREFIX c4n:       <http://vocab.deri.ie/c4n#>
PREFIX dcterms:   <http://purl.org/dc/terms/>
PREFIX foaf:      <http://xmlns.com/foaf/0.1/>
PREFIX loted:     <http://www.loted.eu/ontology#>
PREFIX payment:   <http://reference.data.gov.uk/def/payment#>
PREFIX qb:        <http://purl.org/linked-data/cube#>
PREFIX rdf:       <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX skos:      <http://www.w3.org/2004/02/skos/core#>
PREFIX vann:      <http://purl.org/vocab/vann/>
PREFIX xsd:       <http://www.w3.org/2001/XMLSchema#>
PREFIX conference: <http://conference#>
PREFIX cmt:       <http://cmt#>
PREFIX sigkdd:    <http://sigkdd#>
PREFIX crm:       <http://erlangen-crm.org/current/>
PREFIX id:        <http://collection.britishmuseum.org/id/>
PREFIX idThes:    <http://collection.britishmuseum.org/id/thesauri/>
PREFIX prov:      <http://www.w3.org/ns/prov#>
PREFIX bmo:       <http://collection.britishmuseum.org/id/ontology/>
PREFIX jarql:     <http://jarql.com/>

CONSTRUCT {
    ?E22_Man-Made_Object1 dcterms:identifier ?Object ID.
    ?E22_Man-Made_Object1 rdf:type crm:E22_Man-Made_Object.
    ?E30_Right1 crm:P3_has_note ?Object Rights.
    ?E30_Right1 rdf:type crm:E30_Right.
    ?E35_Title1 rdfs:label ?Object Title.
    ?E35_Title1 rdf:type crm:E35_Title.
    ?E40_Legal_Body1 rdfs:label ?Object Link Source.
    ?E40_Legal_Body1 rdf:type crm:E40_Legal_Body.
    ?E52_Time-Span1 crm:P82_at_some_time_within ?Artist Birth Date.
    ?E52_Time-Span1 rdf:type crm:E52_Time-Span.
    ?E52_Time-Span2 crm:P82_at_some_time_within ?Artist Death Date.
    ?E52_Time-Span2 rdf:type crm:E52_Time-Span.
    ?E52_Time-Span3 crm:P82a_begin_of_the_begin ?Object Earliest Date.
    ?E52_Time-Span3 rdf:type crm:E52_Time-Span.
    ?E52_Time-Span4 crm:P82b_end_of_the_end ?Object Latest Date.
    ?E52_Time-Span4 rdf:type crm:E52_Time-Span.
    ?E54_Dimension1 crm:P3_has_note ?dimensions.
    ?E54_Dimension1 rdf:type crm:E54_Dimension.
    ?E55_Type1 rdfs:label ?Object Work Type.
    ?E55_Type1 rdf:type crm:E55_Type.
    ?E55_Type1 rdfs:label ?Object Facet Value 1.
    ?E55_Type1 rdf:type crm:E55_Type.
    ?E74_Group1 rdfs:label ?Artist Nationality.
    ?E74_Group1 rdf:type crm:E74_Group.
    ?E82_Actor_Appellation1 rdfs:label ?Artist Appellation.
    ?E82_Actor_Appellation1 rdf:type crm:E82_Actor_Appellation.
    ?E21_Person1 rdfs:label ?Artist Name.
    ?E21_Person1 rdf:type crm:E21_Person.
    ?E21_Person1 crm:P130i_features_are_also_found_on ?E22_Man-Made_Object1.
    ?E39_Actor1 crm:P67i_is_referred_to_by ?E30_Right1.
    ?E71_Man-Made_Thing1 crm:P67i_is_referred_to_by ?E35_Title1.
    ?E21_Person1 id:ontology/PX_pupil_of ?E39_Actor1.
    ?E21_Person1 crm:P105_right_held_by ?E40_Legal_Body1.
    ?E54_Dimension1 crm:P84i_was_maximum_duration_of ?E52_Time-Span1.
    ?E54_Dimension1 crm:P84i_was_maximum_duration_of ?E52_Time-Span2.
    ?E54_Dimension1 crm:P84i_was_maximum_duration_of ?E52_Time-Span3.
    ?E54_Dimension1 crm:P84i_was_maximum_duration_of ?E52_Time-Span4.
    ?E70_Thing1 crm:P43_has_dimension ?E54_Dimension1.
    ?E21_Person1 crm:P130i_features_are_also_found_on ?E55_Type1.
    ?E55_Type1 crm:P101i_was_use_of ?E70_Thing1.
    ?E55_Type1 crm:P130i_features_are_also_found_on ?E71_Man-Made_Thing1.
    ?E39_Actor1 id:ontology/PX_profession ?E74_Group1.
    ?E39_Actor1 crm:P1_is_identified_by ?E82_Actor_Appellation1.
}
WHERE {
    ?root a jarql:Root.
    OPTIONAL { ?root jarql:Object ID ?Object ID. }
    OPTIONAL { ?root jarql:Object Rights ?Object Rights. }
    OPTIONAL { ?root jarql:Object Title ?Object Title. }
    OPTIONAL { ?root jarql:Object Link Source ?Object Link Source. }
    OPTIONAL { ?root jarql:Artist Birth Date ?Artist Birth Date. }
    OPTIONAL { ?root jarql:Artist Death Date ?Artist Death Date. }
    OPTIONAL { ?root jarql:Object Earliest Date ?Object Earliest Date. }
    OPTIONAL { ?root jarql:Object Latest Date ?Object Latest Date. }
    OPTIONAL { ?root jarql:dimensions ?dimensions. }
    OPTIONAL { ?root jarql:Object Work Type ?Object Work Type. }
    OPTIONAL { ?root jarql:Object Facet Value 1 ?Object Facet Value 1. }
    OPTIONAL { ?root jarql:Artist Nationality ?Artist Nationality. }
    OPTIONAL { ?root jarql:Artist Appellation ?Artist Appellation. }
    OPTIONAL { ?root jarql:Artist Name ?Artist Name. }
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e22_man-made_object/',?Object ID)) as ?E22_Man-Made_Object1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e40_legal_body/',?Object Link Source)) as ?E40_Legal_Body1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e55_type/',?Object Work Type)) as ?E55_Type1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e74_group/',?Artist Nationality)) as ?E74_Group1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e82_actor_appellation/',?Artist Appellation)) as ?E82_Actor_Appellation1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e21_person/',?Artist Name)) as ?E21_Person1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e39_actor/',?Object Rights)) as ?E39_Actor1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e71_man-made_thing/',?Object Title)) as ?E71_Man-Made_Thing1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e70_thing/',?dimensions)) as ?E70_Thing1)
}