PREFIX schema:    <http://schema.org/>
PREFIX rdfs:      <http://www.w3.org/2000/01/rdf-schema#>
PREFIX pc:        <http://purl.org/procurement/public-contracts#>
PREFIX gr:        <http://purl.org/goodrelations/v1#>
PREFIX owl:       <http://www.w3.org/2002/07/owl#>
PREFIX adms:      <http://www.w3.org/ns/adms#>
PREFIX c4n:       <http://vocab.deri.ie/c4n#>
PREFIX dcterms:   <http://purl.org/dc/terms/>
PREFIX foaf:      <http://xmlns.com/foaf/0.1/>
PREFIX loted:     <http://www.loted.eu/ontology#>
PREFIX payment:   <http://reference.data.gov.uk/def/payment#>
PREFIX qb:        <http://purl.org/linked-data/cube#>
PREFIX rdf:       <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX skos:      <http://www.w3.org/2004/02/skos/core#>
PREFIX vann:      <http://purl.org/vocab/vann/>
PREFIX xsd:       <http://www.w3.org/2001/XMLSchema#>
PREFIX conference: <http://conference#>
PREFIX cmt:       <http://cmt#>
PREFIX sigkdd:    <http://sigkdd#>
PREFIX crm:       <http://erlangen-crm.org/current/>
PREFIX id:        <http://collection.britishmuseum.org/id/>
PREFIX idThes:    <http://collection.britishmuseum.org/id/thesauri/>
PREFIX prov:      <http://www.w3.org/ns/prov#>
PREFIX bmo:       <http://collection.britishmuseum.org/id/ontology/>
PREFIX jarql:     <http://jarql.com/>

CONSTRUCT {
    ?E21_Person1 rdfs:label ?Attribution_URI.
    ?E21_Person1 rdf:type crm:E21_Person.
    ?E22_Man-Made_Object1 crm:P3_has_note ?Description.
    ?E22_Man-Made_Object1 rdf:type crm:E22_Man-Made_Object.
    ?E22_Man-Made_Object2 rdfs:label ?Object_URI.
    ?E22_Man-Made_Object2 rdf:type crm:E22_Man-Made_Object.
    ?E35_Title1 rdfs:label ?Title.
    ?E35_Title1 rdf:type crm:E35_Title.
    ?E52_Time-Span1 crm:P82_at_some_time_within ?Dated.
    ?E52_Time-Span1 rdf:type crm:E52_Time-Span.
    ?E54_Dimension1 crm:P3_has_note ?Dimensions.
    ?E54_Dimension1 rdf:type crm:E54_Dimension.
    ?E55_Type1 rdfs:label ?Culture_URI.
    ?E55_Type1 rdf:type crm:E55_Type.
    ?E55_Type2 rdfs:label ?Culture.
    ?E55_Type2 rdf:type crm:E55_Type.
    ?E55_Type3 rdfs:label ?Type_URI.
    ?E55_Type3 rdf:type crm:E55_Type.
    ?E55_Type4 rdfs:label ?Object Name.
    ?E55_Type4 rdf:type crm:E55_Type.
    ?E55_Type5 rdfs:label ?Medium_URI.
    ?E55_Type5 rdf:type crm:E55_Type.
    ?E55_Type6 rdfs:label ?Medium.
    ?E55_Type6 rdf:type crm:E55_Type.
    ?E82_Actor_Appellation1 rdfs:label ?Attribution_Appellation_URI.
    ?E82_Actor_Appellation1 rdf:type crm:E82_Actor_Appellation.
    ?E82_Actor_Appellation2 rdfs:label ?Attribution.
    ?E82_Actor_Appellation2 rdf:type crm:E82_Actor_Appellation.
    ?E21_Person1 id:ontology/PX_pupil_of ?E39_Actor1.
    ?E82_Actor_Appellation2 crm:P62i_is_depicted_by ?E22_Man-Made_Object1.
    ?E82_Actor_Appellation2 crm:P62i_is_depicted_by ?E22_Man-Made_Object2.
    ?E71_Man-Made_Thing1 crm:P67i_is_referred_to_by ?E35_Title1.
    ?E82_Actor_Appellation2 crm:P1i_identifies ?E39_Actor2.
    ?E54_Dimension1 crm:P84i_was_maximum_duration_of ?E52_Time-Span1.
    ?E70_Thing1 crm:P43_has_dimension ?E54_Dimension1.
    ?E55_Type6 crm:P127i_has_narrower_term ?E55_Type1.
    ?E55_Type6 crm:P127i_has_narrower_term ?E55_Type2.
    ?E55_Type6 crm:P127i_has_narrower_term ?E55_Type3.
    ?E55_Type6 crm:P127i_has_narrower_term ?E55_Type4.
    ?E55_Type6 crm:P127i_has_narrower_term ?E55_Type5.
    ?E82_Actor_Appellation2 crm:P130i_features_are_also_found_on ?E55_Type6.
    ?E55_Type6 crm:P101i_was_use_of ?E70_Thing6.
    ?E55_Type6 crm:P130i_features_are_also_found_on ?E71_Man-Made_Thing6.
    ?E39_Actor1 crm:P1_is_identified_by ?E82_Actor_Appellation1.
}
WHERE {
    ?root a jarql:Root.
    OPTIONAL { ?root jarql:Attribution_URI ?Attribution_URI. }
    OPTIONAL { ?root jarql:Description ?Description. }
    OPTIONAL { ?root jarql:Object_URI ?Object_URI. }
    OPTIONAL { ?root jarql:Title ?Title. }
    OPTIONAL { ?root jarql:Dated ?Dated. }
    OPTIONAL { ?root jarql:Dimensions ?Dimensions. }
    OPTIONAL { ?root jarql:Culture_URI ?Culture_URI. }
    OPTIONAL { ?root jarql:Culture ?Culture. }
    OPTIONAL { ?root jarql:Type_URI ?Type_URI. }
    OPTIONAL { ?root jarql:Object Name ?Object Name. }
    OPTIONAL { ?root jarql:Medium_URI ?Medium_URI. }
    OPTIONAL { ?root jarql:Medium ?Medium. }
    OPTIONAL { ?root jarql:Attribution_Appellation_URI ?Attribution_Appellation_URI. }
    OPTIONAL { ?root jarql:Attribution ?Attribution. }
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e21_person/',?Attribution_URI)) as ?E21_Person1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e22_man-made_object/',?Object_URI)) as ?E22_Man-Made_Object2)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e55_type/',?Culture_URI)) as ?E55_Type1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e55_type/',?Type_URI)) as ?E55_Type3)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e55_type/',?Medium_URI)) as ?E55_Type5)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e82_actor_appellation/',?Attribution_Appellation_URI)) as ?E82_Actor_Appellation1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e39_actor/',?Attribution_URI)) as ?E39_Actor1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e71_man-made_thing/',?Title)) as ?E71_Man-Made_Thing1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e39_actor/',?Attribution)) as ?E39_Actor2)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e70_thing/',?Dimensions)) as ?E70_Thing1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e70_thing/',?Medium)) as ?E70_Thing6)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e71_man-made_thing/',?Medium)) as ?E71_Man-Made_Thing6)
}