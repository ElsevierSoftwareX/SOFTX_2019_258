PREFIX schema:    <http://schema.org/>
PREFIX rdfs:      <http://www.w3.org/2000/01/rdf-schema#>
PREFIX pc:        <http://purl.org/procurement/public-contracts#>
PREFIX gr:        <http://purl.org/goodrelations/v1#>
PREFIX owl:       <http://www.w3.org/2002/07/owl#>
PREFIX adms:      <http://www.w3.org/ns/adms#>
PREFIX c4n:       <http://vocab.deri.ie/c4n#>
PREFIX dcterms:   <http://purl.org/dc/terms/>
PREFIX foaf:      <http://xmlns.com/foaf/0.1/>
PREFIX loted:     <http://www.loted.eu/ontology#>
PREFIX payment:   <http://reference.data.gov.uk/def/payment#>
PREFIX qb:        <http://purl.org/linked-data/cube#>
PREFIX rdf:       <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX skos:      <http://www.w3.org/2004/02/skos/core#>
PREFIX vann:      <http://purl.org/vocab/vann/>
PREFIX xsd:       <http://www.w3.org/2001/XMLSchema#>
PREFIX conference: <http://conference#>
PREFIX cmt:       <http://cmt#>
PREFIX sigkdd:    <http://sigkdd#>
PREFIX crm:       <http://erlangen-crm.org/current/>
PREFIX id:        <http://collection.britishmuseum.org/id/>
PREFIX idThes:    <http://collection.britishmuseum.org/id/thesauri/>
PREFIX prov:      <http://www.w3.org/ns/prov#>
PREFIX bmo:       <http://collection.britishmuseum.org/id/ontology/>
PREFIX aac-ont:   <http://www.americanartcollaborative.org/ontology/>
PREFIX skos2:     <http://www.w3.org/2008/05/skos#>
PREFIX ElementsGr2: <http://rdvocab.info/ElementsGr2/>
PREFIX rdvocab-schema: <http://rdvocab.info/uri/schema/>
PREFIX edm:       <http://www.europeana.eu/schemas/edm/>
PREFIX schema-dig: <http://schema.dig.isi.edu/ontology/>
PREFIX jarql:     <http://jarql.com/>

CONSTRUCT {
    ?E21_Person1 rdfs:label ?item.artist_uri.
    ?E21_Person1 rdf:type crm:E21_Person.
    ?E22_Man-Made_Object1 crm:P3_has_note ?item.description.
    ?E22_Man-Made_Object1 rdf:type crm:E22_Man-Made_Object.
    ?E35_Title1 rdfs:label ?item.title.
    ?E35_Title1 rdf:type crm:E35_Title.
    ?E38_Image1 rdfs:label ?item.image.
    ?E38_Image1 rdf:type crm:E38_Image.
    ?E52_Time-Span1 crm:P82_at_some_time_within ?item.year.
    ?E52_Time-Span1 rdf:type crm:E52_Time-Span.
    ?E55_Type1 rdfs:label ?item.technique_uri.
    ?E55_Type1 rdf:type crm:E55_Type.
    ?E55_Type1 rdfs:label ?item.technique.
    ?E55_Type1 rdf:type crm:E55_Type.
    ?E78_Collection1 rdfs:label ?name.
    ?E78_Collection1 rdf:type crm:E78_Collection.
    ?E82_Actor_Appellation1 rdfs:label ?item.artist_appellation_uri.
    ?E82_Actor_Appellation1 rdf:type crm:E82_Actor_Appellation.
    ?E82_Actor_Appellation1 rdfs:label ?item.artist.
    ?E82_Actor_Appellation1 rdf:type crm:E82_Actor_Appellation.
    ?E21_Person1 id:ontology/PX_pupil_of ?E39_Actor1.
    ?E82_Actor_Appellation1 crm:P62i_is_depicted_by ?E22_Man-Made_Object1.
    ?E71_Man-Made_Thing1 crm:P67i_is_referred_to_by ?E35_Title1.
    ?E82_Actor_Appellation1 crm:P67i_is_referred_to_by ?E38_Image1.
    ?E82_Actor_Appellation1 crm:P1i_identifies ?E39_Actor1.
    ?E54_Dimension1 crm:P84i_was_maximum_duration_of ?E52_Time-Span1.
    ?E70_Thingundefined crm:P43_has_dimension ?E54_Dimensionundefined.
    ?E82_Actor_Appellation1 crm:P130i_features_are_also_found_on ?E55_Type1.
    ?E55_Type1 crm:P101i_was_use_of ?E70_Thing1.
    ?E55_Type1 crm:P130i_features_are_also_found_on ?E71_Man-Made_Thing1.
    ?E39_Actor1 crm:P62i_is_depicted_by ?E78_Collection1.
}
WHERE {
    ?root a jarql:Root.
    OPTIONAL { ?root jarql:item.artist_uri ?item.artist_uri. }
    OPTIONAL { ?root jarql:item.description ?item.description. }
    OPTIONAL { ?root jarql:item.title ?item.title. }
    OPTIONAL { ?root jarql:item.image ?item.image. }
    OPTIONAL { ?root jarql:item.year ?item.year. }
    OPTIONAL { ?root jarql:item.technique_uri ?item.technique_uri. }
    OPTIONAL { ?root jarql:item.technique ?item.technique. }
    OPTIONAL { ?root jarql:name ?name. }
    OPTIONAL { ?root jarql:item.artist_appellation_uri ?item.artist_appellation_uri. }
    OPTIONAL { ?root jarql:item.artist ?item.artist. }
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e21_person/',?item.artist_uri)) as ?E21_Person1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e38_image/',?item.image)) as ?E38_Image1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e55_type/',?item.technique_uri)) as ?E55_Type1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e82_actor_appellation/',?item.artist_appellation_uri)) as ?E82_Actor_Appellation1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e39_actor/',?item.artist_uri)) as ?E39_Actor1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e71_man-made_thing/',?item.title)) as ?E71_Man-Made_Thing1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e54_dimension/',?item.year)) as ?E54_Dimension1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e70_thing/',?item.technique_uri)) as ?E70_Thing1)
}