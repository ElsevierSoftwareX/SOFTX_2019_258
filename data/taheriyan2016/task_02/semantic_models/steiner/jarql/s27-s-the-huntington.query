    ?E52_Time-Span1 crm:P86i_contains ?E52_Time-Span2.
    ?E52_Time-Span1 crm:P86i_contains ?E52_Time-Span3.
    ?E52_Time-Span1 crm:P84_had_at_most_duration ?E54_Dimension1.
