    ?E21_Person1 id:ontology/PX_pupil_of ?E39_Actor1.
    ?E67_Birth1 crm:P8_took_place_on_or_within ?E21_Person1.
    ?E38_Image1 crm:P62i_is_depicted_by ?E22_Man-Made_Object1.
    ?E38_Image1 crm:P67i_is_referred_to_by ?E35_Title1.
    ?E74_Group1 crm:P107_has_current_or_former_member ?E39_Actor1.
    ?E54_Dimension1 crm:P84i_was_maximum_duration_of ?E52_Time-Span1.
    ?E54_Dimension1 crm:P84i_was_maximum_duration_of ?E52_Time-Span2.
    ?E54_Dimension1 crm:P84i_was_maximum_duration_of ?E52_Time-Span3.
    ?E70_Thing1 crm:P101_had_as_general_use ?E55_Type1.
