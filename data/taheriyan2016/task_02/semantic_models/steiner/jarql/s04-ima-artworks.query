    ?E55_Type1 rdfs:label ?materials.
    ?E55_Type1 rdf:type crm:E55_Type.
    ?E82_Actor_Appellation1 rdfs:label ?artistAppellationURI.
    ?E82_Actor_Appellation1 rdf:type crm:E82_Actor_Appellation.
    ?E33_Linguistic_Object2 crm:P3_has_note ?galleryLabel.
    ?E33_Linguistic_Object2 rdf:type crm:E33_Linguistic_Object.
    ?E52_Time-Span2 crm:P82_at_some_time_within ?deathDate.
    ?E52_Time-Span2 rdf:type crm:E52_Time-Span.
    ?E74_Group1 rdfs:label ?nationality.
    ?E74_Group1 rdf:type crm:E74_Group.
    ?E52_Time-Span3 crm:P82_at_some_time_within ?creationDate.
    ?E52_Time-Span3 rdf:type crm:E52_Time-Span.
    ?E21_Person1 rdfs:label ?artistURI.
    ?E21_Person1 rdf:type crm:E21_Person.
    ?E67_Birth1 rdfs:label ?Birth_URI.
    ?E67_Birth1 rdf:type crm:E67_Birth.
    ?E54_Dimension1 crm:P3_has_note ?dimensions.
    ?E54_Dimension1 rdf:type crm:E54_Dimension.
    ?E33_Linguistic_Object1 crm:P3_has_note ?provenance.
    ?E33_Linguistic_Object1 rdf:type crm:E33_Linguistic_Object.
    ?E55_Type2 rdfs:label ?provenanceTypeURI.
    ?E55_Type2 rdf:type crm:E55_Type.
    ?E39_Actorundefined crm:P51i_is_former_or_current_owner_of ?E18_Physical_Thing.
    ?E67_Birth1 crm:P8_took_place_on_or_within ?E21_Person1.
    ?E55_Type2 crm:P62i_is_depicted_by ?E22_Man-Made_Object1.
    ?E55_Type2 crm:P67i_is_referred_to_by ?E33_Linguistic_Object1.
    ?E33_Linguistic_Object1 crm:P73i_is_translation_of ?E33_Linguistic_Object2.
    ?E71_Man-Made_Thing1 crm:P67i_is_referred_to_by ?E35_Title1.
    ?E55_Type2 crm:P67i_is_referred_to_by ?E38_Image1.
    ?E21_Person1 id:ontology/PX_pupil_of ?E39_Actor1.
    ?E54_Dimension1 crm:P84i_was_maximum_duration_of ?E52_Time-Span1.
    ?E54_Dimension1 crm:P84i_was_maximum_duration_of ?E52_Time-Span2.
    ?E54_Dimension1 crm:P84i_was_maximum_duration_of ?E52_Time-Span3.
    ?E70_Thing1 crm:P43_has_dimension ?E54_Dimension1.
    ?E55_Type2 crm:P127i_has_narrower_term ?E55_Type1.
    ?E55_Type2 crm:P127i_has_narrower_term ?E55_Type3.
    ?E55_Type2 crm:P12i_was_present_at ?E67_Birth1.
    ?E21_Person1 crm:P8i_witnessed ?E69_Death1.
    ?E55_Type2 crm:P101i_was_use_of ?E70_Thing2.
    ?E55_Type2 crm:P130i_features_are_also_found_on ?E71_Man-Made_Thing2.
    ?E39_Actor1 id:ontology/PX_profession ?E74_Group1.
    ?E39_Actor1 crm:P1_is_identified_by ?E82_Actor_Appellation1.
    ?E18_Physical_Thing1 crm:P24i_changed_ownership_through ?E8_Acquisition1.
