WHERE {
    ?root a jarql:Root.
    OPTIONAL { ?root jarql:imageURL ?imageURL. }
    OPTIONAL { ?root jarql:Death_URI ?Death_URI. }
    OPTIONAL { ?root jarql:birthDate ?birthDate. }
    OPTIONAL { ?root jarql:artistFirstLast ?artistFirstLast. }
    OPTIONAL { ?root jarql:materialsURI ?materialsURI. }
    OPTIONAL { ?root jarql:nationalityURI ?nationalityURI. }
    OPTIONAL { ?root jarql:title ?title. }
    OPTIONAL { ?root jarql:creditLine ?creditLine. }
    OPTIONAL { ?root jarql:galleryLabelTypeURI ?galleryLabelTypeURI. }
    OPTIONAL { ?root jarql:sourceURL ?sourceURL. }
    OPTIONAL { ?root jarql:materials ?materials. }
    OPTIONAL { ?root jarql:artistAppellationURI ?artistAppellationURI. }
    OPTIONAL { ?root jarql:galleryLabel ?galleryLabel. }
    OPTIONAL { ?root jarql:deathDate ?deathDate. }
    OPTIONAL { ?root jarql:nationality ?nationality. }
    OPTIONAL { ?root jarql:creationDate ?creationDate. }
    OPTIONAL { ?root jarql:artistURI ?artistURI. }
    OPTIONAL { ?root jarql:Birth_URI ?Birth_URI. }
    OPTIONAL { ?root jarql:dimensions ?dimensions. }
    OPTIONAL { ?root jarql:provenance ?provenance. }
    OPTIONAL { ?root jarql:provenanceTypeURI ?provenanceTypeURI. }
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e38_image/',?imageURL)) as ?E38_Image1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e69_death/',?Death_URI)) as ?E69_Death1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e55_type/',?materialsURI)) as ?E55_Type1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e74_group/',?nationalityURI)) as ?E74_Group1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e55_type/',?galleryLabelTypeURI)) as ?E55_Type3)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e22_man-made_object/',?sourceURL)) as ?E22_Man-Made_Object1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e82_actor_appellation/',?artistAppellationURI)) as ?E82_Actor_Appellation1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e21_person/',?artistURI)) as ?E21_Person1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e67_birth/',?Birth_URI)) as ?E67_Birth1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e55_type/',?provenanceTypeURI)) as ?E55_Type2)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e71_man-made_thing/',?title)) as ?E71_Man-Made_Thing1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e39_actor/',?artistURI)) as ?E39_Actor1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e70_thing/',?dimensions)) as ?E70_Thing1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e70_thing/',?provenanceTypeURI)) as ?E70_Thing2)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e71_man-made_thing/',?provenanceTypeURI)) as ?E71_Man-Made_Thing2)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e18_physical_thing/',?creditLine)) as ?E18_Physical_Thing1)
}