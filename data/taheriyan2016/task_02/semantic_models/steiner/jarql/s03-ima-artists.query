CONSTRUCT {
    ?E74_Group1 rdfs:label ?nationality uri.
    ?E74_Group1 rdf:type crm:E74_Group.
    ?E74_Group1 rdfs:label ?nationality.
    ?E74_Group1 rdf:type crm:E74_Group.
    ?E67_Birth1 rdfs:label ?Birth_URI.
    ?E67_Birth1 rdf:type crm:E67_Birth.
    ?E35_Title1 rdfs:label ?title.
    ?E35_Title1 rdf:type crm:E35_Title.
    ?E52_Time-Span1 crm:P82_at_some_time_within ?birthDate.
    ?E52_Time-Span1 rdf:type crm:E52_Time-Span.
    ?E21_Person1 rdfs:label ?Artist URI.
    ?E21_Person1 rdf:type crm:E21_Person.
    ?E69_Death1 rdfs:label ?Death_URI.
    ?E69_Death1 rdf:type crm:E69_Death.
    ?E82_Actor_Appellation1 rdfs:label ?artist appellation uri.
    ?E82_Actor_Appellation1 rdf:type crm:E82_Actor_Appellation.
    ?E82_Actor_Appellation1 rdfs:label ?name first last.
    ?E82_Actor_Appellation1 rdf:type crm:E82_Actor_Appellation.
    ?E52_Time-Span2 crm:P82_at_some_time_within ?deathDate.
    ?E52_Time-Span2 rdf:type crm:E52_Time-Span.
    ?E21_Person1 id:ontology/PX_pupil_of ?E39_Actor1.
    ?E69_Death1 crm:P12_occurred_in_the_presence_of ?E21_Person1.
    ?E52_Time-Span2 crm:P1_is_identified_by ?E35_Title1.
    ?E82_Actor_Appellation1 crm:P1i_identifies ?E39_Actor1.
    ?E52_Time-Span2 crm:P86i_contains ?E52_Time-Span1.
    ?E21_Person1 crm:P92i_was_brought_into_existence_by ?E67_Birth1.
    ?E39_Actor1 id:ontology/PX_profession ?E74_Group1.
    ?E52_Time-Span2 crm:P1_is_identified_by ?E82_Actor_Appellation1.
}
WHERE {
    ?root a jarql:Root.
    OPTIONAL { ?root jarql:nationality uri ?nationality uri. }
    OPTIONAL { ?root jarql:nationality ?nationality. }
    OPTIONAL { ?root jarql:Birth_URI ?Birth_URI. }
    OPTIONAL { ?root jarql:title ?title. }
    OPTIONAL { ?root jarql:birthDate ?birthDate. }
    OPTIONAL { ?root jarql:Artist URI ?Artist URI. }
    OPTIONAL { ?root jarql:Death_URI ?Death_URI. }
    OPTIONAL { ?root jarql:artist appellation uri ?artist appellation uri. }
    OPTIONAL { ?root jarql:name first last ?name first last. }
    OPTIONAL { ?root jarql:deathDate ?deathDate. }
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e74_group/',?nationality uri)) as ?E74_Group1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e67_birth/',?Birth_URI)) as ?E67_Birth1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e21_person/',?Artist URI)) as ?E21_Person1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e69_death/',?Death_URI)) as ?E69_Death1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e82_actor_appellation/',?artist appellation uri)) as ?E82_Actor_Appellation1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e39_actor/',?Artist URI)) as ?E39_Actor1)
}