###  http://conference#amount

conference:amount rdf:type owl:DatatypeProperty ;

        rdfs:label "amount in USD" ;

        rdfs:domain conference:Conference_fees .



###  http://conference#has_a_URL

conference:has_a_URL rdf:type owl:DatatypeProperty ,
                    owl:FunctionalProperty ;

           rdfs:label "has URL" ;

           rdfs:domain conference:Conference_www ;

           rdfs:range xsd:string .



###  http://conference#has_a_date_of_issue

conference:has_a_date_of_issue rdf:type owl:DatatypeProperty ,
                              owl:FunctionalProperty ;

                     rdfs:label "has issue date" ;

                     rdfs:domain conference:Conference_document ;

                     rdfs:range xsd:date .



###  http://conference#has_a_degree

conference:has_a_degree rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;

              rdfs:label "has degree" ;

              rdfs:comment "A weight factor specifying the degree of preference." ;

              rdfs:domain conference:Review_preference ;

              rdfs:range xsd:int .



###  http://conference#has_a_location

conference:has_a_location rdf:type owl:DatatypeProperty ;

                rdfs:label "has location" ;

                rdfs:comment "Name of conference location (city, country)." ;

                rdfs:domain conference:Conference_volume ;

                rdfs:range xsd:string .



###  http://conference#has_a_name

conference:has_a_name rdf:type owl:DatatypeProperty ,
                     owl:FunctionalProperty ;

            rdfs:label "has name" ;

            rdfs:range xsd:string ;

            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( :Committee
                                        :Conference
                                        :Conference_part
                                        :Conference_proceedings
                                        :Organization
                                        :Publisher
                                        :Topic
                                      )
                        ] .



###  http://conference#has_a_paper_title

conference:has_a_paper_title rdf:type owl:DatatypeProperty ;

                   rdfs:label "title" ;

                   rdfs:domain [ rdf:type owl:Class ;
                                 owl:unionOf ( :Paper
                                               :Reviewed_contribution
                                             )
                               ] .



###  http://conference#has_a_volume

conference:has_a_volume rdf:type owl:DatatypeProperty ,
                       owl:FunctionalProperty ;

              rdfs:label "has volume" ;

              rdfs:domain conference:Conference_proceedings ;

              rdfs:range xsd:int .



###  http://conference#has_an_ISBN

conference:has_an_ISBN rdf:type owl:DatatypeProperty ,
                      owl:FunctionalProperty ;

             rdfs:label "has ISBN" ;

             rdfs:domain conference:Conference_proceedings ;

             rdfs:range xsd:string .



###  http://conference#has_an_email

conference:has_an_email rdf:type owl:DatatypeProperty ;

              rdfs:label "has e-mail" ;

              rdfs:domain conference:Person ;

              rdfs:range xsd:string .



###  http://conference#has_an_expertise

conference:has_an_expertise rdf:type owl:DatatypeProperty ,
                           owl:FunctionalProperty ;

                  rdfs:label "has level of expertise" ;

                  rdfs:domain conference:Review_expertise ;

                  rdfs:range [ rdf:type rdfs:Datatype ;
                               owl:oneOf [ rdf:type rdf:List ;
                                           rdf:first "high"^^xsd:string ;
                                           rdf:rest [ rdf:type rdf:List ;
                                                      rdf:first "low"^^xsd:string ;
                                                      rdf:rest [ rdf:type rdf:List ;
                                                                 rdf:first "medium"^^xsd:string ;
                                                                 rdf:rest [ rdf:type rdf:List ;
                                                                            rdf:first "very high"^^xsd:string ;
                                                                            rdf:rest rdf:nil
                                                                          ]
                                                               ]
                                                    ]
                                         ]
                             ] .



###  http://conference#has_detailed_comments

conference:has_detailed_comments rdf:type owl:DatatypeProperty ;

                       rdfs:label "review comments" ;

                       rdfs:domain conference:Review .



###  http://conference#has_gender

conference:has_gender rdf:type owl:DatatypeProperty ;

            rdfs:label "has gender" ;

            rdfs:domain conference:Person ;

            rdfs:range [ rdf:type rdfs:Datatype ;
                         owl:oneOf [ rdf:type rdf:List ;
                                     rdf:first "female"^^xsd:string ;
                                     rdf:rest [ rdf:type rdf:List ;
                                                rdf:first "male"^^xsd:string ;
                                                rdf:rest rdf:nil
                                              ]
                                   ]
                       ] .



###  http://conference#has_text

conference:has_text rdf:type owl:DatatypeProperty ;

          rdfs:label "abstract text" ;

          rdfs:comment "Abstract (as plain text)" ;

          rdfs:domain conference:Extended_abstract .



