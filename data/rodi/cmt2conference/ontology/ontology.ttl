@prefix : <http://conference#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix conference: <http://conference#> .

<http://conference> rdf:type owl:Ontology .


#################################################################
#
#    Datatypes
#
#################################################################


###  http://www.w3.org/2001/XMLSchema#date

xsd:date rdf:type rdfs:Datatype .





#################################################################
#
#    Object Properties
#
#################################################################


###  http://conference#belong_to_a_conference_volume

conference:belong_to_a_conference_volume rdf:type owl:InverseFunctionalProperty ,
                                        owl:ObjectProperty ;

                               rdfs:label "date belongs to conference" ;

                               rdfs:range conference:Conference_volume ;

                               rdfs:domain conference:Important_dates .



###  http://conference#belongs_to_a_review_reference

conference:belongs_to_a_review_reference rdf:type owl:InverseFunctionalProperty ,
                                        owl:ObjectProperty ;

                               rdfs:label "topic of review preference" ;

                               rdfs:range conference:Review_preference ;

                               rdfs:domain conference:Topic .



###  http://conference#belongs_to_reviewers

conference:belongs_to_reviewers rdf:type owl:ObjectProperty ;

                      rdfs:label "belongs to reviewers" ;

                      rdfs:range conference:Reviewer ;

                      owl:inverseOf conference:has_a_review_reference_or_expertise ;

                      rdfs:domain [ rdf:type owl:Class ;
                                    owl:unionOf ( :Review_expertise
                                                  :Review_preference
                                                )
                                  ] .



###  http://conference#contributes

conference:contributes rdf:type owl:ObjectProperty ;

             rdfs:label "contributes" ;

             rdfs:range conference:Conference_document ;

             rdfs:domain conference:Person .



###  http://conference#gives_presentations

conference:gives_presentations rdf:type owl:ObjectProperty ;

                     rdfs:label "gives presentation" ;

                     rdfs:domain conference:Active_conference_participant ;

                     rdfs:range conference:Presentation ;

                     owl:inverseOf conference:is_given_by .



###  http://conference#has_a_committee_chair

conference:has_a_committee_chair rdf:type owl:FunctionalProperty ,
                                owl:ObjectProperty ;

                       rdfs:label "has committee chair" ;

                       rdfs:range conference:Chair ;

                       rdfs:domain conference:Committee ;

                       rdfs:subPropertyOf :has_members .



###  http://conference#has_a_committee_co-chair

conference:has_a_committee_co-chair rdf:type owl:ObjectProperty ;

                          rdfs:label "has committee co-chair" ;

                          rdfs:range conference:Co-chair ;

                          rdfs:domain conference:Committee ;

                          rdfs:subPropertyOf :has_members ;

                          owl:inverseOf conference:was_a_committe_co-chair_of .



###  http://conference#has_a_commtitee

conference:has_a_commtitee rdf:type owl:ObjectProperty ;

                 rdfs:label "has committee" ;

                 rdfs:range conference:Committee ;

                 rdfs:domain conference:Conference_volume .



###  http://conference#has_a_program_committee

conference:has_a_program_committee rdf:type owl:FunctionalProperty ,
                                  owl:ObjectProperty ;

                         rdfs:domain conference:Conference_volume ;

                         rdfs:range conference:Program_committee ;

                         rdfs:subPropertyOf :has_a_commtitee .



###  http://conference#has_a_publisher

conference:has_a_publisher rdf:type owl:FunctionalProperty ,
                          owl:ObjectProperty ;

                 rdfs:label "has publisher" ;

                 rdfs:domain conference:Conference_proceedings ;

                 rdfs:range conference:Publisher .



###  http://conference#has_a_review

conference:has_a_review rdf:type owl:InverseFunctionalProperty ,
                       owl:ObjectProperty ;

              rdfs:label "has review" ;

              rdfs:range conference:Review ;

              rdfs:domain conference:Reviewed_contribution ;

              owl:inverseOf conference:reviews .



###  http://conference#has_a_review_expertise

conference:has_a_review_expertise rdf:type owl:InverseFunctionalProperty ,
                                 owl:ObjectProperty ;

                        rdfs:label "reviewed with expertise level" ;

                        rdfs:range conference:Review_expertise ;

                        rdfs:domain conference:Submitted_contribution ;

                        owl:inverseOf conference:has_a_submitted_contribution .



###  http://conference#has_a_review_reference_or_expertise

conference:has_a_review_reference_or_expertise rdf:type owl:ObjectProperty ;

                                     rdfs:label "reviewer has reference or expertise" ;

                                     rdfs:domain conference:Reviewer ;

                                     rdfs:range [ rdf:type owl:Class ;
                                                  owl:unionOf ( :Review_expertise
                                                                :Review_preference
                                                              )
                                                ] .



###  http://conference#has_a_steering_committee

conference:has_a_steering_committee rdf:type owl:FunctionalProperty ,
                                   owl:ObjectProperty ;

                          rdfs:domain conference:Conference_volume ;

                          rdfs:range conference:Steering_committee ;

                          rdfs:subPropertyOf :has_a_commtitee .



###  http://conference#has_a_submitted_contribution

conference:has_a_submitted_contribution rdf:type owl:FunctionalProperty ,
                                       owl:ObjectProperty ;

                              rdfs:label "has submitted topic" ;

                              rdfs:domain conference:Review_expertise ;

                              rdfs:range conference:Submitted_contribution .



###  http://conference#has_a_topic_or_a_submission_contribution

conference:has_a_topic_or_a_submission_contribution rdf:type owl:FunctionalProperty ,
                                                   owl:ObjectProperty ;

                                          rdfs:label "prefers topic/contribution" ;

                                          rdfs:domain conference:Review_preference ;

                                          rdfs:range [ rdf:type owl:Class ;
                                                       owl:unionOf ( :Submitted_contribution
                                                                     :Topic
                                                                   )
                                                     ] .



###  http://conference#has_a_track-workshop-tutorial_chair

conference:has_a_track-workshop-tutorial_chair rdf:type owl:FunctionalProperty ,
                                              owl:ObjectProperty ;

                                     rdfs:label "has chair" ;

                                     rdfs:domain conference:Conference_part ;

                                     rdfs:range conference:Track-workshop_chair ;

                                     owl:inverseOf conference:was_a_track-workshop_chair_of .



###  http://conference#has_a_track-workshop-tutorial_topic

conference:has_a_track-workshop-tutorial_topic rdf:type owl:ObjectProperty ;

                                     rdfs:label "has topic" ;

                                     rdfs:domain conference:Conference_part ;

                                     rdfs:range conference:Topic ;

                                     owl:inverseOf conference:is_a_topis_of_conference_parts .



###  http://conference#has_an_abstract

conference:has_an_abstract rdf:type owl:FunctionalProperty ,
                          owl:ObjectProperty ;

                 rdfs:label "has abstract" ;

                 rdfs:range conference:Abstract ;

                 owl:inverseOf conference:is_the_1th_part_of ;

                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( :Paper
                                             :Poster
                                             :Presentation
                                           )
                             ] .



###  http://conference#has_an_organizing_committee

conference:has_an_organizing_committee rdf:type owl:FunctionalProperty ,
                                      owl:ObjectProperty ;

                             rdfs:domain conference:Conference_volume ;

                             rdfs:range conference:Organizing_committee ;

                             rdfs:subPropertyOf :has_a_commtitee .



###  http://conference#has_authors

conference:has_authors rdf:type owl:ObjectProperty ;

             rdfs:label "has author" ;

             rdfs:domain conference:Conference_document ;

             rdfs:range conference:Person ;

             owl:inverseOf conference:contributes .



###  http://conference#has_been_assigned_a_review_reference

conference:has_been_assigned_a_review_reference rdf:type owl:InverseFunctionalProperty ,
