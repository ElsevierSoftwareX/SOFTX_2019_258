conference:is_an_abstract_submission_date rdf:type owl:DatatypeProperty ,
                                         owl:FunctionalProperty ;

                                rdfs:label "abstract submission deadline" ;

                                rdfs:domain conference:Important_dates ;

                                rdfs:range xsd:date .



