                     rdfs:range conference:Important_dates ;

                     owl:inverseOf conference:belong_to_a_conference_volume .



###  http://conference#has_members

conference:has_members rdf:type owl:ObjectProperty ;

             rdfs:label "has member" ;

             rdfs:domain conference:Committee ;

             rdfs:range conference:Committee_member .



###  http://conference#has_parts

conference:has_parts rdf:type owl:ObjectProperty ;

           rdfs:label "has part" ;

           rdfs:range conference:Conference_part ;

