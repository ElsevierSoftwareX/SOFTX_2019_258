#################################################################
#
#    Classes
#
#################################################################


###  http://conference#Abstract

conference:Abstract rdf:type owl:Class ;

          rdfs:label "Abstract" ;

          rdfs:subClassOf conference:Extended_abstract .



###  http://conference#Accepted_contribution

conference:Accepted_contribution rdf:type owl:Class ;

                       rdfs:subClassOf conference:Reviewed_contribution ;

                       owl:disjointWith :Rejected_contribution .



###  http://conference#Active_conference_participant

conference:Active_conference_participant rdf:type owl:Class ;

                               rdfs:subClassOf conference:Conference_contributor ,
                                               :Conference_participant ,
                                               [ rdf:type owl:Class ;
                                                 owl:unionOf ( :Invited_speaker
                                                               :Regular_author
                                                             )
                                               ] ;

                               owl:disjointWith :Passive_conference_participant .



###  http://conference#Call_for_paper

conference:Call_for_paper rdf:type owl:Class ;

                rdfs:label "Call for papers" ;

                rdfs:subClassOf conference:Conference_document .



###  http://conference#Call_for_participation

conference:Call_for_participation rdf:type owl:Class ;

                        rdfs:label "Call for participation" ;

                        rdfs:subClassOf conference:Conference_document .



###  http://conference#Camera_ready_contribution

conference:Camera_ready_contribution rdf:type owl:Class ;

                           rdfs:subClassOf conference:Accepted_contribution .



###  http://conference#Chair

conference:Chair rdf:type owl:Class ;

       owl:equivalentClass [ rdf:type owl:Restriction ;
                             owl:onProperty :was_a_committee_chair_of ;
                             owl:someValuesFrom :Committee
                           ] ;

       rdfs:subClassOf conference:Committee_member .



###  http://conference#Co-chair

conference:Co-chair rdf:type owl:Class ;

          owl:equivalentClass [ rdf:type owl:Restriction ;
                                owl:onProperty :was_a_committe_co-chair_of ;
                                owl:someValuesFrom :Committee
                              ] ;

          rdfs:subClassOf conference:Committee_member .



###  http://conference#Committee

conference:Committee rdf:type owl:Class ;

           rdfs:label "Conference Committee" ;

           owl:equivalentClass [ rdf:type owl:Class ;
                                 owl:unionOf ( :Organizing_committee
                                               :Program_committee
                                               :Steering_committee
                                             )
                               ] ;

           rdfs:comment "Any conference committee, such as, e.g., the program committee." .



###  http://conference#Committee_member

conference:Committee_member rdf:type owl:Class ;

                  rdfs:label "Committee member" ;

                  owl:equivalentClass [ rdf:type owl:Restriction ;
                                        owl:onProperty :was_a_member_of ;
                                        owl:someValuesFrom :Committee
                                      ] ;

                  rdfs:subClassOf conference:Person .



###  http://conference#Conference

conference:Conference rdf:type owl:Class ;

            rdfs:label "Conference" ;

            rdfs:comment "A (scientific) conference" .



###  http://conference#Conference_announcement

conference:Conference_announcement rdf:type owl:Class ;

                         rdfs:label "Conference announcement" ;

                         rdfs:subClassOf conference:Conference_document ;

                         rdfs:comment "Announcements made on behalf of the conference." .



###  http://conference#Conference_applicant

conference:Conference_applicant rdf:type owl:Class ;

