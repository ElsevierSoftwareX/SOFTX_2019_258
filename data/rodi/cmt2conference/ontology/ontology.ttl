           owl:inverseOf conference:is_part_of_conference_volumes .



###  http://conference#has_tracks

conference:has_tracks rdf:type owl:ObjectProperty ;

            rdfs:domain conference:Conference_volume ;

            rdfs:range conference:Track ;

            rdfs:subPropertyOf :has_parts .



###  http://conference#has_tutorials

conference:has_tutorials rdf:type owl:ObjectProperty ;

               rdfs:domain conference:Conference_volume ;

               rdfs:range conference:Tutorial ;

               rdfs:subPropertyOf :has_parts .



###  http://conference#has_workshops

conference:has_workshops rdf:type owl:ObjectProperty ;

               rdfs:domain conference:Conference_volume ;

               rdfs:range conference:Workshop ;

               rdfs:subPropertyOf :has_parts .



###  http://conference#invited_by

conference:invited_by rdf:type owl:ObjectProperty ;

            rdfs:label "invited by" ;

            rdfs:domain conference:Reviewer ;

            rdfs:range conference:Reviewer .



###  http://conference#invites_co-reviewers

conference:invites_co-reviewers rdf:type owl:ObjectProperty ;

                      rdfs:label "has invited for co-review" ;

                      rdfs:range conference:Reviewer ;

                      rdfs:domain conference:Reviewer ;

                      owl:inverseOf conference:invited_by .



###  http://conference#is_a_topis_of_conference_parts

conference:is_a_topis_of_conference_parts rdf:type owl:ObjectProperty ;

                                rdfs:label "is topic of" ;

                                rdfs:range conference:Conference_part ;

                                rdfs:domain conference:Topic .



###  http://conference#is_given_by

conference:is_given_by rdf:type owl:ObjectProperty ;

             rdfs:label "given by" ;

             rdfs:range conference:Active_conference_participant ;

             rdfs:domain conference:Presentation .



###  http://conference#is_part_of_conference_volumes

conference:is_part_of_conference_volumes rdf:type owl:ObjectProperty ;

                               rdfs:label "is part of" ;

                               rdfs:domain conference:Conference_part ;

                               rdfs:range conference:Conference_volume .



###  http://conference#is_submitted_at

conference:is_submitted_at rdf:type owl:ObjectProperty ;

                 rdfs:label "submitted at" ;

                 rdfs:domain conference:Conference_contribution ;

                 rdfs:range conference:Conference_volume ;

                 owl:inverseOf conference:has_contributions .



###  http://conference#is_the_1th_part_of

conference:is_the_1th_part_of rdf:type owl:InverseFunctionalProperty ,
                             owl:ObjectProperty ;

                    rdfs:label "is first part of" ;

                    rdfs:domain conference:Abstract ;

                    rdfs:range [ rdf:type owl:Class ;
                                 owl:unionOf ( :Paper
                                               :Poster
                                               :Presentation
                                             )
                               ] .



###  http://conference#issues

conference:issues rdf:type owl:InverseFunctionalProperty ,
                 owl:ObjectProperty ;

        rdfs:label "publishes" ;

        rdfs:range conference:Conference_proceedings ;

        rdfs:domain conference:Publisher ;

        owl:inverseOf conference:has_a_publisher .



###  http://conference#reviews

conference:reviews rdf:type owl:FunctionalProperty ,
                  owl:ObjectProperty ;

         rdfs:label "is review for" ;

         rdfs:domain conference:Review ;

         rdfs:range conference:Reviewed_contribution .



###  http://conference#was_a_committe_co-chair_of

conference:was_a_committe_co-chair_of rdf:type owl:ObjectProperty ;

                            rdfs:domain conference:Co-chair ;

                            rdfs:range conference:Committee ;

                            rdfs:subPropertyOf :was_a_member_of .



###  http://conference#was_a_committee_chair_of

conference:was_a_committee_chair_of rdf:type owl:InverseFunctionalProperty ,
                                   owl:ObjectProperty ;

                          rdfs:domain conference:Chair ;

                          rdfs:range conference:Committee ;

                          owl:inverseOf conference:has_a_committee_chair ;

                          rdfs:subPropertyOf :was_a_member_of .



###  http://conference#was_a_committee_of

conference:was_a_committee_of rdf:type owl:ObjectProperty ;

                    rdfs:domain conference:Committee ;

                    rdfs:range conference:Conference_volume ;

                    owl:inverseOf conference:has_a_commtitee .



###  http://conference#was_a_member_of

conference:was_a_member_of rdf:type owl:ObjectProperty ;

                 rdfs:range conference:Committee ;

                 rdfs:domain conference:Committee_member ;

                 owl:inverseOf conference:has_members .



###  http://conference#was_a_program_committee_of

conference:was_a_program_committee_of rdf:type owl:InverseFunctionalProperty ,
                                     owl:ObjectProperty ;

                            rdfs:range conference:Conference_volume ;

                            rdfs:domain conference:Program_committee ;

                            owl:inverseOf conference:has_a_program_committee ;

                            rdfs:subPropertyOf :was_a_committee_of .



###  http://conference#was_a_steering_committee_of

conference:was_a_steering_committee_of rdf:type owl:InverseFunctionalProperty ,
                                      owl:ObjectProperty ;

                             rdfs:range conference:Conference_volume ;

                             rdfs:domain conference:Steering_committee ;

                             owl:inverseOf conference:has_a_steering_committee ;

                             rdfs:subPropertyOf :was_a_committee_of .



###  http://conference#was_a_track-workshop_chair_of

conference:was_a_track-workshop_chair_of rdf:type owl:InverseFunctionalProperty ,
                                        owl:ObjectProperty ;

                               rdfs:range conference:Conference_part ;

                               rdfs:domain conference:Track-workshop_chair .



###  http://conference#was_an_organizing_committee_of

conference:was_an_organizing_committee_of rdf:type owl:InverseFunctionalProperty ,
                                         owl:ObjectProperty ;

                                rdfs:range conference:Conference_volume ;

                                rdfs:domain conference:Organizing_committee ;

                                owl:inverseOf conference:has_an_organizing_committee ;

                                rdfs:subPropertyOf :was_a_committee_of .





