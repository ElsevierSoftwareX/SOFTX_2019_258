PREFIX schema:    <http://schema.org/>
PREFIX rdfs:      <http://www.w3.org/2000/01/rdf-schema#>
PREFIX pc:        <http://purl.org/procurement/public-contracts#>
PREFIX gr:        <http://purl.org/goodrelations/v1#>
PREFIX owl:       <http://www.w3.org/2002/07/owl#>
PREFIX adms:      <http://www.w3.org/ns/adms#>
PREFIX c4n:       <http://vocab.deri.ie/c4n#>
PREFIX dcterms:   <http://purl.org/dc/terms/>
PREFIX foaf:      <http://xmlns.com/foaf/0.1/>
PREFIX loted:     <http://www.loted.eu/ontology#>
PREFIX payment:   <http://reference.data.gov.uk/def/payment#>
PREFIX qb:        <http://purl.org/linked-data/cube#>
PREFIX rdf:       <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX skos:      <http://www.w3.org/2004/02/skos/core#>
PREFIX vann:      <http://purl.org/vocab/vann/>
PREFIX xsd:       <http://www.w3.org/2001/XMLSchema#>
PREFIX conference: <http://conference#>
PREFIX jarql:     <http://jarql.com/>

CONSTRUCT {
    ?Paper0 dcterms:identifier ?co_write_paper__paper.
    ?Paper0 rdf:type conference:Paper.
    ?Contribution_co_author0 dcterms:identifier ?co_write_paper__co_author.
    ?Contribution_co_author0 rdf:type conference:Contribution_co_author.
    ?Contribution_co_author0 conference:contributes ?Paper0.
}
WHERE {
    jarql:root jarql:co_write_paper ?co_write_paper.
    ?co_write_paper jarql:paper ?co_write_paper__paper.
    jarql:root jarql:co_write_paper ?co_write_paper.
    ?co_write_paper jarql:co_author ?co_write_paper__co_author.
    BIND (URI(CONCAT('http://conference/paper/',?co_write_paper__paper)) as ?Paper0)
    BIND (URI(CONCAT('http://conference/contribution_co_author/',?co_write_paper__co_author)) as ?Contribution_co_author0)
}