PREFIX schema:    <http://schema.org/>
PREFIX rdfs:      <http://www.w3.org/2000/01/rdf-schema#>
PREFIX pc:        <http://purl.org/procurement/public-contracts#>
PREFIX gr:        <http://purl.org/goodrelations/v1#>
PREFIX owl:       <http://www.w3.org/2002/07/owl#>
PREFIX adms:      <http://www.w3.org/ns/adms#>
PREFIX c4n:       <http://vocab.deri.ie/c4n#>
PREFIX dcterms:   <http://purl.org/dc/terms/>
PREFIX foaf:      <http://xmlns.com/foaf/0.1/>
PREFIX loted:     <http://www.loted.eu/ontology#>
PREFIX payment:   <http://reference.data.gov.uk/def/payment#>
PREFIX qb:        <http://purl.org/linked-data/cube#>
PREFIX rdf:       <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX skos:      <http://www.w3.org/2004/02/skos/core#>
PREFIX vann:      <http://purl.org/vocab/vann/>
PREFIX xsd:       <http://www.w3.org/2001/XMLSchema#>
PREFIX conference: <http://conference#>
PREFIX cmt:       <http://cmt#>
PREFIX jarql:     <http://jarql.com/>

CONSTRUCT {
    ?Conference_contribution0 dcterms:identifier ?contributes__Conference_document.
    ?Conference_contribution0 rdf:type conference:Conference_contribution.
    ?Conference_contributor0 dcterms:identifier ?contributes__Person.
    ?Conference_contributor0 rdf:type conference:Conference_contributor.
    ?Conference_contributor0 conference:contributes ?Conference_contribution0.
}
WHERE {
    jarql:root jarql:contributes ?contributes.
    ?contributes jarql:Conference_document ?contributes__Conference_document.
    jarql:root jarql:contributes ?contributes.
    ?contributes jarql:Person ?contributes__Person.
    BIND (URI(CONCAT('http://conference/conference_contribution/',?contributes__Conference_document)) as ?Conference_contribution0)
    BIND (URI(CONCAT('http://conference/conference_contributor/',?contributes__Person)) as ?Conference_contributor0)
}