PREFIX schema:    <http://schema.org/>
PREFIX rdfs:      <http://www.w3.org/2000/01/rdf-schema#>
PREFIX pc:        <http://purl.org/procurement/public-contracts#>
PREFIX gr:        <http://purl.org/goodrelations/v1#>
PREFIX owl:       <http://www.w3.org/2002/07/owl#>
PREFIX adms:      <http://www.w3.org/ns/adms#>
PREFIX c4n:       <http://vocab.deri.ie/c4n#>
PREFIX dcterms:   <http://purl.org/dc/terms/>
PREFIX foaf:      <http://xmlns.com/foaf/0.1/>
PREFIX loted:     <http://www.loted.eu/ontology#>
PREFIX payment:   <http://reference.data.gov.uk/def/payment#>
PREFIX qb:        <http://purl.org/linked-data/cube#>
PREFIX rdf:       <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX skos:      <http://www.w3.org/2004/02/skos/core#>
PREFIX vann:      <http://purl.org/vocab/vann/>
PREFIX xsd:       <http://www.w3.org/2001/XMLSchema#>
PREFIX conference: <http://conference#>
PREFIX cmt:       <http://cmt#>
PREFIX jarql:     <http://jarql.com/>

CONSTRUCT {
    ?Conference0 dcterms:identifier ?person_conference__cid.
    ?Conference0 rdf:type cmt:Conference.
    ?Person0 dcterms:identifier ?person_conference__pid.
    ?Person0 rdf:type cmt:Person.
    ?ConferenceMember0 cmt:memberOfConference ?Conference0.
    ?ConferenceMember0 rdfs:subClassOf owl:Thing.
    ?Person0 cmt:hasConflictOfInterest ?Document0.
    ?Document1 rdfs:subClassOf owl:Thing.
}
WHERE {
    jarql:root jarql:person_conference ?person_conference.
    ?person_conference jarql:cid ?person_conference__cid.
    jarql:root jarql:person_conference ?person_conference.
    ?person_conference jarql:pid ?person_conference__pid.
    BIND (URI(CONCAT('http://cmt/conference/',?person_conference__cid)) as ?Conference0)
    BIND (URI(CONCAT('http://cmt/person/',?person_conference__pid)) as ?Person0)
    BIND (URI(CONCAT('http://cmt/conferencemember/',?person_conference__cid)) as ?ConferenceMember0)
    BIND (URI(CONCAT('http://cmt/document/',?person_conference__pid)) as ?Document0)
}