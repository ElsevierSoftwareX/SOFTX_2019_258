PREFIX jarql:     <http://jarql.com/>

CONSTRUCT {
    ?Paper0 dcterms:identifier ?co_writePaper__Paper.
    ?Paper0 rdf:type cmt:Paper.
    ?Co_author0 dcterms:identifier ?co_writePaper__Co_author.
    ?Co_author0 rdf:type cmt:Co_author.
    ?Paper0 cmt:hasCo_author ?Co_author0.
    ?Co_author0 cmt:co_writePaper ?Paper0.
    ?Paper0 cmt:acceptedBy ?Administrator0.
    ?Paper0 cmt:rejectedBy ?Administrator0.
    ?Administrator0 cmt:acceptPaper ?Paper0.
    ?Administrator0 cmt:rejectPaper ?Paper0.
    ?Paper0 cmt:hasBid ?Bid0.
    ?Paper0 cmt:assignedTo ?Reviewer0.
    ?Paper0 cmt:readByReviewer ?Reviewer0.
    ?Reviewer0 cmt:hasBeenAssigned ?Paper0.
    ?Reviewer0 cmt:readPaper ?Paper0.
    ?Paper0 cmt:hasAuthor ?Author0.
    ?Author0 cmt:submitPaper ?Paper0.
    ?Author0 cmt:writePaper ?Paper0.
    ?Paper0 cmt:hasDecision ?Decision0.
    ?Paper0 cmt:hasSubjectArea ?SubjectArea0.
    ?Paper0 cmt:readByMeta_Reviewer ?Meta_Reviewer0.
    ?Administrator0 cmt:assignReviewer ?Reviewer5555.
    ?Reviewer5 cmt:assignedByAdministrator ?Administrator0000.
    ?Bid4 cmt:adjustedBy ?Reviewer5555.
    ?Reviewer5 cmt:adjustBid ?Bid4.
    ?Co_author0 cmt:hasConflictOfInterest ?Paper0.
    ?Administrator0 cmt:hasConflictOfInterest ?Paper0.
    ?Reviewer0 cmt:hasConflictOfInterest ?Paper0.
    ?Author0 cmt:hasConflictOfInterest ?Paper0.
    ?Meta_Reviewer0 cmt:hasConflictOfInterest ?Paper0.
}
WHERE {
    jarql:root jarql:co_writePaper ?co_writePaper.
    ?co_writePaper jarql:Paper ?co_writePaper__Paper.
    jarql:root jarql:co_writePaper ?co_writePaper.
    ?co_writePaper jarql:Co_author ?co_writePaper__Co_author.
    BIND (URI(CONCAT('http://cmt/paper/',?co_writePaper__Paper)) as ?Paper0)
    BIND (URI(CONCAT('http://cmt/co_author/',?co_writePaper__Co_author)) as ?Co_author0)
    BIND (URI(CONCAT('http://cmt/administrator/',?co_writePaper__Paper)) as ?Administrator0)
    BIND (URI(CONCAT('http://cmt/bid/',?co_writePaper__Paper)) as ?Bid0)
    BIND (URI(CONCAT('http://cmt/reviewer/',?co_writePaper__Paper)) as ?Reviewer0)
    BIND (URI(CONCAT('http://cmt/author/',?co_writePaper__Paper)) as ?Author0)
    BIND (URI(CONCAT('http://cmt/decision/',?co_writePaper__Paper)) as ?Decision0)
    BIND (URI(CONCAT('http://cmt/subjectarea/',?co_writePaper__Paper)) as ?SubjectArea0)
    BIND (URI(CONCAT('http://cmt/meta_reviewer/',?co_writePaper__Paper)) as ?Meta_Reviewer0)
}