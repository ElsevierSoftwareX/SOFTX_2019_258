PREFIX schema:    <http://schema.org/>
PREFIX rdfs:      <http://www.w3.org/2000/01/rdf-schema#>
PREFIX pc:        <http://purl.org/procurement/public-contracts#>
PREFIX gr:        <http://purl.org/goodrelations/v1#>
PREFIX owl:       <http://www.w3.org/2002/07/owl#>
PREFIX adms:      <http://www.w3.org/ns/adms#>
PREFIX c4n:       <http://vocab.deri.ie/c4n#>
PREFIX dcterms:   <http://purl.org/dc/terms/>
PREFIX foaf:      <http://xmlns.com/foaf/0.1/>
PREFIX loted:     <http://www.loted.eu/ontology#>
PREFIX payment:   <http://reference.data.gov.uk/def/payment#>
PREFIX qb:        <http://purl.org/linked-data/cube#>
PREFIX rdf:       <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX skos:      <http://www.w3.org/2004/02/skos/core#>
PREFIX vann:      <http://purl.org/vocab/vann/>
PREFIX xsd:       <http://www.w3.org/2001/XMLSchema#>
PREFIX conference: <http://conference#>
PREFIX cmt:       <http://cmt#>
PREFIX sigkdd:    <http://sigkdd#>
PREFIX jarql:     <http://jarql.com/>

CONSTRUCT {
    ?Conference0 dcterms:identifier ?hasConferenceMember__Conference.
    ?Conference0 rdf:type cmt:Conference.
    ?ConferenceMember0 dcterms:identifier ?hasConferenceMember__ConferenceMember.
    ?ConferenceMember0 rdf:type cmt:ConferenceMember.
    ?Conference0 cmt:hasConferenceMember ?ConferenceMember0.
    ?ConferenceMember0 cmt:memberOfConference ?Conference0.
    ?Conference0 cmt:detailsEnteredBy ?Administrator0.
    ?Conference0 cmt:virtualMeetingEnabledBy ?Administrator0.
    ?Conference0 cmt:paperAssignmentFinalizedBy ?Administrator0.
    ?Conference0 cmt:hardcopyMailingManifestsPrintedBy ?Administrator0.
    ?Conference0 cmt:paperAssignmentToolsRunBy ?Administrator0.
    ?Conference0 cmt:reviewCriteriaEnteredBy ?Administrator0.
    ?Conference0 cmt:reviewerBiddingStartedBy ?Administrator0.
    ?Administrator0 cmt:enableVirtualMeeting ?Conference0.
    ?Administrator0 cmt:enterConferenceDetails ?Conference0.
    ?Administrator0 cmt:enterReviewCriteria ?Conference0.
    ?Administrator0 cmt:finalizePaperAssignment ?Conference0.
    ?Administrator0 cmt:printHardcopyMailingManifests ?Conference0.
    ?Administrator0 cmt:startReviewerBidding ?Conference0.
    ?Administrator0 cmt:runPaperAssignmentTools ?Conference0.
}
WHERE {
    jarql:root jarql:hasConferenceMember ?hasConferenceMember.
    ?hasConferenceMember jarql:Conference ?hasConferenceMember__Conference.
    jarql:root jarql:hasConferenceMember ?hasConferenceMember.
    ?hasConferenceMember jarql:ConferenceMember ?hasConferenceMember__ConferenceMember.
    BIND (URI(CONCAT('http://cmt/conference/',?hasConferenceMember__Conference)) as ?Conference0)
    BIND (URI(CONCAT('http://cmt/conferencemember/',?hasConferenceMember__ConferenceMember)) as ?ConferenceMember0)
    BIND (URI(CONCAT('http://cmt/administrator/',?hasConferenceMember__Conference)) as ?Administrator0)
}