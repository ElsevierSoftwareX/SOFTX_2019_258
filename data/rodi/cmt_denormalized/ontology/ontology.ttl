                 rdfs:subClassOf http://cmt#ConferenceMember ;

                 owl:disjointWith http://cmt#ConferenceChair .



###  http://cmt#Author

http://cmt#Author rdf:type owl:Class ;

        rdfs:label "Author"@en ;

        rdfs:subClassOf http://cmt#ConferenceMember ,
                        http://cmt#User .



###  http://cmt#AuthorNotReviewer

http://cmt#AuthorNotReviewer rdf:type owl:Class ;

                   rdfs:label "Author not Reviewer"@en ;

                   rdfs:subClassOf http://cmt#Author ;

                   owl:disjointWith http://cmt#ProgramCommitteeMember ,
                                    http://cmt#Reviewer ;

                   rdfs:comment "A person who is an author but not a reviewer"@en .



###  http://cmt#Bid

http://cmt#Bid rdf:type owl:Class ;

     rdfs:label "Bid"@en ;

     owl:disjointWith http://cmt#Conference ,
                      http://cmt#Decision ,
                      http://cmt#Document ,
                      http://cmt#Person ,
                      http://cmt#Preference ,
                      http://cmt#ProgramCommittee .



###  http://cmt#Chairman

http://cmt#Chairman rdf:type owl:Class ;

          rdfs:label "Chairman"@en ;

          owl:equivalentClass [ rdf:type owl:Class ;
                                owl:unionOf ( http://cmt#AssociatedChair
                                              http://cmt#ConferenceChair
                                              http://cmt#ProgramCommitteeChair
                                            )
                              ] ;

          rdfs:subClassOf http://cmt#Person .



###  http://cmt#Co-author

http://cmt#Co-author rdf:type owl:Class ;

           rdfs:label "Co-author"@en ;

           rdfs:subClassOf http://cmt#Author .



###  http://cmt#Conference

http://cmt#Conference rdf:type owl:Class ;

            rdfs:label "Conference"@en ;

            rdfs:subClassOf owl:Thing ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty http://cmt#reviewsPerPaper ;
                              owl:cardinality "1"^^xsd:nonNegativeInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty http://cmt#name ;
                              owl:cardinality "1"^^xsd:nonNegativeInteger
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty http://cmt#date ;
                              owl:minCardinality "1"^^xsd:nonNegativeInteger
                            ] ;

            owl:disjointWith http://cmt#Decision ,
                             http://cmt#Document ,
                             http://cmt#Person ,
                             http://cmt#Preference ,
                             http://cmt#ProgramCommittee .



###  http://cmt#ConferenceChair

http://cmt#ConferenceChair rdf:type owl:Class ;

                 rdfs:label "Conference Chair"@en ;

                 rdfs:subClassOf http://cmt#ConferenceMember .



###  http://cmt#ConferenceMember

http://cmt#ConferenceMember rdf:type owl:Class ;

                  rdfs:label "Conference Member"@en ;

                  rdfs:subClassOf http://cmt#Person .



