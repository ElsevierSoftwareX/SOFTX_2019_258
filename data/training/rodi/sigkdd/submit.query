PREFIX schema:    <http://schema.org/>
PREFIX rdfs:      <http://www.w3.org/2000/01/rdf-schema#>
PREFIX pc:        <http://purl.org/procurement/public-contracts#>
PREFIX gr:        <http://purl.org/goodrelations/v1#>
PREFIX owl:       <http://www.w3.org/2002/07/owl#>
PREFIX adms:      <http://www.w3.org/ns/adms#>
PREFIX c4n:       <http://vocab.deri.ie/c4n#>
PREFIX dcterms:   <http://purl.org/dc/terms/>
PREFIX foaf:      <http://xmlns.com/foaf/0.1/>
PREFIX loted:     <http://www.loted.eu/ontology#>
PREFIX payment:   <http://reference.data.gov.uk/def/payment#>
PREFIX qb:        <http://purl.org/linked-data/cube#>
PREFIX rdf:       <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX skos:      <http://www.w3.org/2004/02/skos/core#>
PREFIX vann:      <http://purl.org/vocab/vann/>
PREFIX xsd:       <http://www.w3.org/2001/XMLSchema#>
PREFIX conference: <http://conference#>
PREFIX cmt:       <http://cmt#>
PREFIX sigkdd:    <http://sigkdd#>
PREFIX jarql:     <http://jarql.com/>

CONSTRUCT {
    ?Author0 dcterms:identifier ?submit__Author.
    ?Author0 rdf:type sigkdd:Author.
    ?Paper0 dcterms:identifier ?submit__Paper.
    ?Paper0 rdf:type sigkdd:Paper.
    ?Author0 sigkdd:submit ?Paper0.
}
WHERE {
    jarql:root jarql:submit ?submit.
    ?submit jarql:Author ?submit__Author.
    jarql:root jarql:submit ?submit.
    ?submit jarql:Paper ?submit__Paper.
    BIND (URI(CONCAT('http://sigkdd/author/',?submit__Author)) as ?Author0)
    BIND (URI(CONCAT('http://sigkdd/paper/',?submit__Paper)) as ?Paper0)
}
