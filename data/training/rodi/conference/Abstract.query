PREFIX schema:    <http://schema.org/>
PREFIX rdfs:      <http://www.w3.org/2000/01/rdf-schema#>
PREFIX pc:        <http://purl.org/procurement/public-contracts#>
PREFIX gr:        <http://purl.org/goodrelations/v1#>
PREFIX owl:       <http://www.w3.org/2002/07/owl#>
PREFIX adms:      <http://www.w3.org/ns/adms#>
PREFIX c4n:       <http://vocab.deri.ie/c4n#>
PREFIX dcterms:   <http://purl.org/dc/terms/>
PREFIX foaf:      <http://xmlns.com/foaf/0.1/>
PREFIX loted:     <http://www.loted.eu/ontology#>
PREFIX payment:   <http://reference.data.gov.uk/def/payment#>
PREFIX qb:        <http://purl.org/linked-data/cube#>
PREFIX rdf:       <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX skos:      <http://www.w3.org/2004/02/skos/core#>
PREFIX vann:      <http://purl.org/vocab/vann/>
PREFIX xsd:       <http://www.w3.org/2001/XMLSchema#>
PREFIX conference: <http://conference#>
PREFIX cmt:       <http://cmt#>
PREFIX jarql:     <http://jarql.com/>

CONSTRUCT {
    ?Abstract0 dcterms:identifier ?Abstract__ID.
    ?Abstract0 rdf:type conference:Abstract.
    ?Paper0 dcterms:identifier ?Abstract__is_the_1th_part_of.
    ?Paper0 rdf:type conference:Paper.
    ?Paper0 conference:Abstract ?Abstract0.
}
WHERE {
    jarql:root jarql:Abstract ?Abstract.
    ?Abstract jarql:ID ?Abstract__ID.
    jarql:root jarql:Abstract ?Abstract.
    ?Abstract jarql:is_the_1th_part_of ?Abstract__is_the_1th_part_of.
    BIND (URI(CONCAT('http://conference/abstract/',?Abstract__ID)) as ?Abstract0)
    BIND (URI(CONCAT('http://conference/paper/',?Abstract__is_the_1th_part_of)) as ?Paper0)
}
