PREFIX schema:    <http://schema.org/>
PREFIX rdfs:      <http://www.w3.org/2000/01/rdf-schema#>
PREFIX pc:        <http://purl.org/procurement/public-contracts#>
PREFIX gr:        <http://purl.org/goodrelations/v1#>
PREFIX owl:       <http://www.w3.org/2002/07/owl#>
PREFIX adms:      <http://www.w3.org/ns/adms#>
PREFIX c4n:       <http://vocab.deri.ie/c4n#>
PREFIX dcterms:   <http://purl.org/dc/terms/>
PREFIX foaf:      <http://xmlns.com/foaf/0.1/>
PREFIX loted:     <http://www.loted.eu/ontology#>
PREFIX payment:   <http://reference.data.gov.uk/def/payment#>
PREFIX qb:        <http://purl.org/linked-data/cube#>
PREFIX rdf:       <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX skos:      <http://www.w3.org/2004/02/skos/core#>
PREFIX vann:      <http://purl.org/vocab/vann/>
PREFIX xsd:       <http://www.w3.org/2001/XMLSchema#>
PREFIX conference: <http://conference#>
PREFIX cmt:       <http://cmt#>
PREFIX jarql:     <http://jarql.com/>

CONSTRUCT {
    ?Reviewer0 dcterms:identifier ?Review__ID.
    ?Reviewer0 rdf:type conference:Reviewer.
    ?Reviewed_contribution0 dcterms:identifier ?Review__reviews.
    ?Reviewed_contribution0 rdf:type conference:Reviewed_contribution.
    ?Reviewed_contribution0 conference:has_a_review ?Reviewer0.
}
WHERE {
    jarql:root jarql:Review ?Review.
    ?Review jarql:ID ?Review__ID.
    jarql:root jarql:Review ?Review.
    ?Review jarql:reviews ?Review__reviews.
    BIND (URI(CONCAT('http://conference/reviewer/',?Review__ID)) as ?Reviewer0)
    BIND (URI(CONCAT('http://conference/reviewed_contribution/',?Review__reviews)) as ?Reviewed_contribution0)
}
