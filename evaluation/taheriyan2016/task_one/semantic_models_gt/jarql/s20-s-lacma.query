WHERE {
    ?root a jarql:Root.
    OPTIONAL { ?root jarql:Artwork_in_Focus ?Artwork_in_Focus. }
    OPTIONAL { ?root jarql:Artist_URI ?Artist_URI. }
    OPTIONAL { ?root jarql:Artwork_Details ?Artwork_Details. }
    OPTIONAL { ?root jarql:Object_URI ?Object_URI. }
    OPTIONAL { ?root jarql:Title ?Title. }
    OPTIONAL { ?root jarql:Image_URL ?Image_URL. }
    OPTIONAL { ?root jarql:Residence ?Residence. }
    OPTIONAL { ?root jarql:Production_Year ?Production_Year. }
    OPTIONAL { ?root jarql:birth_date ?birth_date. }
    OPTIONAL { ?root jarql:death_date ?death_date. }
    OPTIONAL { ?root jarql:Classification_URI ?Classification_URI. }
    OPTIONAL { ?root jarql:Classification ?Classification. }
    OPTIONAL { ?root jarql:Artist_Appellation_URI ?Artist_Appellation_URI. }
    OPTIONAL { ?root jarql:Artist_Name ?Artist_Name. }
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e12_production/',?Artwork_in_Focus)) as ?E12_Production1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e69_death/',?Artwork_in_Focus)) as ?E69_Death1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e53_place/',?Artwork_in_Focus)) as ?E53_Place1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e67_birth/',?Artwork_in_Focus)) as ?E67_Birth1)
