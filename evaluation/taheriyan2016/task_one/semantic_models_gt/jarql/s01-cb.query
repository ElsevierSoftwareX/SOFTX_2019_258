PREFIX schema:    <http://schema.org/>
PREFIX rdfs:      <http://www.w3.org/2000/01/rdf-schema#>
PREFIX pc:        <http://purl.org/procurement/public-contracts#>
PREFIX gr:        <http://purl.org/goodrelations/v1#>
PREFIX owl:       <http://www.w3.org/2002/07/owl#>
PREFIX adms:      <http://www.w3.org/ns/adms#>
PREFIX c4n:       <http://vocab.deri.ie/c4n#>
PREFIX dcterms:   <http://purl.org/dc/terms/>
PREFIX foaf:      <http://xmlns.com/foaf/0.1/>
PREFIX loted:     <http://www.loted.eu/ontology#>
PREFIX payment:   <http://reference.data.gov.uk/def/payment#>
PREFIX qb:        <http://purl.org/linked-data/cube#>
PREFIX rdf:       <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX skos:      <http://www.w3.org/2004/02/skos/core#>
PREFIX vann:      <http://purl.org/vocab/vann/>
PREFIX xsd:       <http://www.w3.org/2001/XMLSchema#>
PREFIX conference: <http://conference#>
PREFIX cmt:       <http://cmt#>
PREFIX sigkdd:    <http://sigkdd#>
PREFIX crm:       <http://erlangen-crm.org/current/>
PREFIX id:        <http://collection.britishmuseum.org/id/>
PREFIX idThes:    <http://collection.britishmuseum.org/id/thesauri/>
PREFIX prov:      <http://www.w3.org/ns/prov#>
PREFIX bmo:       <http://collection.britishmuseum.org/id/ontology/>
PREFIX jarql:     <http://jarql.com/>

CONSTRUCT {
    ?E21_Person1 http://isi.edu/integration/karma/dev#classLink ?Actor_URI.
    ?E21_Person1 rdf:type crm:E21_Person.
    ?E35_Title1 rdfs:label ?Title.
    ?E35_Title1 rdf:type crm:E35_Title.
    ?E52_Time-Span1 crm:P82_at_some_time_within ?Begin Date.
    ?E52_Time-Span1 rdf:type crm:E52_Time-Span.
    ?E52_Time-Span2 crm:P82_at_some_time_within ?End Date.
    ?E52_Time-Span2 rdf:type crm:E52_Time-Span.
    ?E52_Time-Span3 crm:P82_at_some_time_within ?Dated.
    ?E52_Time-Span3 rdf:type crm:E52_Time-Span.
    ?E54_Dimension1 crm:P3_has_note ?Dimensions.
    ?E54_Dimension1 rdf:type crm:E54_Dimension.
    ?E55_Type1 http://isi.edu/integration/karma/dev#classLink ?Medium_URI.
    ?E55_Type1 rdf:type crm:E55_Type.
    ?E55_Type2 rdfs:label ?Medium.
    ?E55_Type2 rdf:type crm:E55_Type.
    ?E82_Actor_Appellation1 http://isi.edu/integration/karma/dev#classLink ?Artist_Appellation_URI.
    ?E82_Actor_Appellation1 rdf:type crm:E82_Actor_Appellation.
    ?E82_Actor_Appellation2 rdfs:label ?Attribution.
    ?E82_Actor_Appellation2 rdf:type crm:E82_Actor_Appellation.
    ?E12_Production1 crm:P14_carried_out_by ?E21_Person1.
    ?E12_Production1 crm:P32_used_general_technique ?E55_Type1.
    ?E12_Production1 crm:P4_has_time-span ?E52_Time-Span3.
    ?E21_Person1 crm:P100i_died_in ?E69_Death1.
    ?E21_Person1 crm:P131_is_identified_by ?E82_Actor_Appellation1.
    ?E21_Person1 crm:P98i_was_born ?E67_Birth1.
    ?E22_Man-Made_Object1 crm:P102_has_title ?E35_Title1.
    ?E22_Man-Made_Object1 crm:P108i_was_produced_by ?E12_Production1.
    ?E22_Man-Made_Object1 crm:P43_has_dimension ?E54_Dimension1.
    ?E67_Birth1 crm:P4_has_time-span ?E52_Time-Span1.
    ?E69_Death1 crm:P4_has_time-span ?E52_Time-Span2.
}
WHERE {
    ?root a jarql:Root.
    OPTIONAL { ?root jarql:Actor_URI ?Actor_URI. }
    OPTIONAL { ?root jarql:Title ?Title. }
    OPTIONAL { ?root jarql:Begin Date ?Begin Date. }
    OPTIONAL { ?root jarql:End Date ?End Date. }
    OPTIONAL { ?root jarql:Dated ?Dated. }
    OPTIONAL { ?root jarql:Dimensions ?Dimensions. }
    OPTIONAL { ?root jarql:Medium_URI ?Medium_URI. }
    OPTIONAL { ?root jarql:Medium ?Medium. }
    OPTIONAL { ?root jarql:Artist_Appellation_URI ?Artist_Appellation_URI. }
    OPTIONAL { ?root jarql:Attribution ?Attribution. }
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e21_person/',?Actor_URI)) as ?E21_Person1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e55_type/',?Medium_URI)) as ?E55_Type1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e82_actor_appellation/',?Artist_Appellation_URI)) as ?E82_Actor_Appellation1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e12_production/',?Actor_URI)) as ?E12_Production1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e69_death/',?Actor_URI)) as ?E69_Death1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e67_birth/',?Actor_URI)) as ?E67_Birth1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e22_man-made_object/',?Title)) as ?E22_Man-Made_Object1)
}