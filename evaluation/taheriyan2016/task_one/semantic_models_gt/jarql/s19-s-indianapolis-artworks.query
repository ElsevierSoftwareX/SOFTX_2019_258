    OPTIONAL { ?root jarql:provenance_type_uri ?provenance_type_uri. }
    OPTIONAL { ?root jarql:galleryLabel_type_uri ?galleryLabel_type_uri. }
    OPTIONAL { ?root jarql:nationality_URI ?nationality_URI. }
