    BIND (URI(CONCAT('http://erlangen-crm.org/current//e69_death/',?artist_uri)) as ?E69_Death1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e67_birth/',?artist_uri)) as ?E67_Birth1)
