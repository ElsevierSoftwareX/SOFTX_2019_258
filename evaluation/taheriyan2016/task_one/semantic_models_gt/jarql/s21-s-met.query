PREFIX bmo:       <http://collection.britishmuseum.org/id/ontology/>
PREFIX jarql:     <http://jarql.com/>

CONSTRUCT {
    ?E21_Person1 http://isi.edu/integration/karma/dev#classLink ?artistURI.
    ?E21_Person1 rdf:type crm:E21_Person.
    ?E22_Man-Made_Object1 http://isi.edu/integration/karma/dev#classLink ?objectURI.
    ?E22_Man-Made_Object1 rdf:type crm:E22_Man-Made_Object.
    ?E35_Title1 rdfs:label ?artworkTitle.
    ?E35_Title1 rdf:type crm:E35_Title.
    ?E52_Time-Span1 crm:P82_at_some_time_within ?birthDate.
    ?E52_Time-Span1 rdf:type crm:E52_Time-Span.
    ?E52_Time-Span2 crm:P82_at_some_time_within ?deathDate.
    ?E52_Time-Span2 rdf:type crm:E52_Time-Span.
    ?E52_Time-Span3 crm:P82a_begin_of_the_begin ?dateOfStartOfCreation.
    ?E52_Time-Span3 rdf:type crm:E52_Time-Span.
    ?E52_Time-Span4 crm:P82b_end_of_the_end ?dateOfEndOfCreation.
    ?E52_Time-Span4 rdf:type crm:E52_Time-Span.
    ?E54_Dimension1 crm:P3_has_note ?dimensions.
    ?E54_Dimension1 rdf:type crm:E54_Dimension.
    ?E55_Type1 http://isi.edu/integration/karma/dev#classLink ?medium_URI.
    ?E55_Type1 rdf:type crm:E55_Type.
    ?E55_Type2 rdfs:label ?medium.
    ?E55_Type2 rdf:type crm:E55_Type.
    ?E55_Type3 http://isi.edu/integration/karma/dev#classLink ?classification_URI.
    ?E55_Type3 rdf:type crm:E55_Type.
    ?E55_Type4 rdfs:label ?classification.
    ?E55_Type4 rdf:type crm:E55_Type.
    ?E74_Group1 http://isi.edu/integration/karma/dev#classLink ?nationality_URI.
    ?E74_Group1 rdf:type crm:E74_Group.
    ?E74_Group2 rdfs:label ?nationality.
    ?E74_Group2 rdf:type crm:E74_Group.
    ?E82_Actor_Appellation1 http://isi.edu/integration/karma/dev#classLink ?artistAppellationURI.
    ?E82_Actor_Appellation1 rdf:type crm:E82_Actor_Appellation.
    ?E82_Actor_Appellation2 rdfs:label ?artistName.
    ?E82_Actor_Appellation2 rdf:type crm:E82_Actor_Appellation.
    ?E8_Acquisition1 crm:P3_has_note ?creditLine.
    ?E8_Acquisition1 rdf:type crm:E8_Acquisition.
    ?E12_Production1 crm:P14_carried_out_by ?E21_Person1.
    ?E12_Production1 crm:P32_used_general_technique ?E55_Type1.
    ?E12_Production1 crm:P4_has_time-span ?E52_Time-Span3.
    ?E21_Person1 crm:P100i_died_in ?E69_Death1.
    ?E21_Person1 crm:P107i_is_current_or_former_member_of ?E74_Group1.
    ?E21_Person1 crm:P131_is_identified_by ?E82_Actor_Appellation1.
    ?E21_Person1 crm:P98i_was_born ?E67_Birth1.
    ?E22_Man-Made_Object1 crm:P102_has_title ?E35_Title1.
    ?E22_Man-Made_Object1 crm:P108i_was_produced_by ?E12_Production1.
    ?E22_Man-Made_Object1 crm:P24i_changed_ownership_through ?E8_Acquisition1.
    ?E22_Man-Made_Object1 crm:P2_has_type ?E55_Type2.
    ?E22_Man-Made_Object1 crm:P43_has_dimension ?E54_Dimension1.
    ?E67_Birth1 crm:P4_has_time-span ?E52_Time-Span1.
    ?E69_Death1 crm:P4_has_time-span ?E52_Time-Span2.
}
WHERE {
    ?root a jarql:Root.
    OPTIONAL { ?root jarql:artistURI ?artistURI. }
    OPTIONAL { ?root jarql:objectURI ?objectURI. }
    OPTIONAL { ?root jarql:artworkTitle ?artworkTitle. }
    OPTIONAL { ?root jarql:birthDate ?birthDate. }
    OPTIONAL { ?root jarql:deathDate ?deathDate. }
    OPTIONAL { ?root jarql:dateOfStartOfCreation ?dateOfStartOfCreation. }
    OPTIONAL { ?root jarql:dateOfEndOfCreation ?dateOfEndOfCreation. }
    OPTIONAL { ?root jarql:dimensions ?dimensions. }
    OPTIONAL { ?root jarql:medium_URI ?medium_URI. }
    OPTIONAL { ?root jarql:medium ?medium. }
    OPTIONAL { ?root jarql:classification_URI ?classification_URI. }
    OPTIONAL { ?root jarql:classification ?classification. }
    OPTIONAL { ?root jarql:nationality_URI ?nationality_URI. }
    OPTIONAL { ?root jarql:nationality ?nationality. }
    OPTIONAL { ?root jarql:artistAppellationURI ?artistAppellationURI. }
    OPTIONAL { ?root jarql:artistName ?artistName. }
    OPTIONAL { ?root jarql:creditLine ?creditLine. }
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e12_production/',?artistURI)) as ?E12_Production1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e69_death/',?artistURI)) as ?E69_Death1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e67_birth/',?artistURI)) as ?E67_Birth1)
}