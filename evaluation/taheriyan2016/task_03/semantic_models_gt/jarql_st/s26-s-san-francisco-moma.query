CONSTRUCT {
    ?Place2 rdfs:label ?art-work__deathPlaceLabel.
    ?Place2 rdf:type aac-ont:Place.
    ?Person1 ElementsGr2:countryAssociatedWithThePerson ?art-work__nationality.
    ?Person1 rdf:type aac-ont:Person.
    ?WebResource1 rdfs:label ?art-work__image-URL.
    ?WebResource1 rdf:type edm:WebResource.
    ?CulturalHeritageObject1 dcterms:provenance ?art-work__credit.
    ?CulturalHeritageObject1 rdf:type aac-ont:CulturalHeritageObject.
    ?CulturalHeritageObject1 aac-ont:accessionNumber ?art-work__accession-id.
    ?CulturalHeritageObject1 rdf:type aac-ont:CulturalHeritageObject.
    ?CulturalHeritageObject1 dcterms:rightsHolder ?art-work__copyright.
    ?CulturalHeritageObject1 rdf:type aac-ont:CulturalHeritageObject.
    ?CulturalHeritageObject1 dcterms:extent ?art-work__dimensions.
    ?CulturalHeritageObject1 rdf:type aac-ont:CulturalHeritageObject.
    ?Concept1 skos2:prefLabel ?art-work__type.
    ?Concept1 rdf:type skos2:Concept.
    ?Person1 ElementsGr2:dateOfBirth ?art-work__birthYear.
    ?Person1 rdf:type aac-ont:Person.
    ?Person1 ElementsGr2:dateOfDeath ?art-work__deathYear.
    ?Person1 rdf:type aac-ont:Person.
    ?Document1 rdfs:label ?art-work__url.
    ?Document1 rdf:type foaf:Document.
    ?Person1 ElementsGr2:nameOfThePerson ?art-work__artistName.
    ?Person1 rdf:type aac-ont:Person.
    ?CulturalHeritageObject1 dcterms:title ?art-work__title.
    ?CulturalHeritageObject1 rdf:type aac-ont:CulturalHeritageObject.
    ?Place1 rdfs:label ?art-work__birthPlaceLabel.
    ?Place1 rdf:type aac-ont:Place.
}
WHERE {
    ?root a jarql:Root.
    OPTIONAL { ?root jarql:art-work ?art-work. }
    OPTIONAL { ?art-work jarql:deathPlaceLabel ?art-work__deathPlaceLabel. }
    OPTIONAL { ?root jarql:art-work ?art-work. }
    OPTIONAL { ?art-work jarql:nationality ?art-work__nationality. }
    OPTIONAL { ?root jarql:art-work ?art-work. }
    OPTIONAL { ?art-work jarql:image-URL ?art-work__image-URL. }
    OPTIONAL { ?root jarql:art-work ?art-work. }
    OPTIONAL { ?art-work jarql:credit ?art-work__credit. }
    OPTIONAL { ?root jarql:art-work ?art-work. }
    OPTIONAL { ?art-work jarql:accession-id ?art-work__accession-id. }
    OPTIONAL { ?root jarql:art-work ?art-work. }
    OPTIONAL { ?art-work jarql:copyright ?art-work__copyright. }
    OPTIONAL { ?root jarql:art-work ?art-work. }
    OPTIONAL { ?art-work jarql:dimensions ?art-work__dimensions. }
    OPTIONAL { ?root jarql:art-work ?art-work. }
    OPTIONAL { ?art-work jarql:type ?art-work__type. }
    OPTIONAL { ?root jarql:art-work ?art-work. }
    OPTIONAL { ?art-work jarql:birthYear ?art-work__birthYear. }
    OPTIONAL { ?root jarql:art-work ?art-work. }
    OPTIONAL { ?art-work jarql:deathYear ?art-work__deathYear. }
    OPTIONAL { ?root jarql:art-work ?art-work. }
    OPTIONAL { ?art-work jarql:url ?art-work__url. }
    OPTIONAL { ?root jarql:art-work ?art-work. }
    OPTIONAL { ?art-work jarql:artistName ?art-work__artistName. }
    OPTIONAL { ?root jarql:art-work ?art-work. }
    OPTIONAL { ?art-work jarql:title ?art-work__title. }
    OPTIONAL { ?root jarql:art-work ?art-work. }
    OPTIONAL { ?art-work jarql:birthPlaceLabel ?art-work__birthPlaceLabel. }
    BIND (URI(CONCAT('http://purl.org/vocab/frbr/core/place/',?art-work__deathPlaceLabel)) as ?Place2)
    BIND (URI(CONCAT('http://purl.org/vocab/frbr/core/person/',?art-work__nationality)) as ?Person1)
    BIND (URI(CONCAT('http://www.europeana.eu/schemas/edm//webresource/',?art-work__image-URL)) as ?WebResource1)
    BIND (URI(CONCAT('http://www.americanartcollaborative.org/ontology//culturalheritageobject/',?art-work__credit)) as ?CulturalHeritageObject1)
    BIND (URI(CONCAT('http://purl.org/vocab/frbr/core/concept/',?art-work__type)) as ?Concept1)
    BIND (URI(CONCAT('http://xmlns.com/foaf/0.1//document/',?art-work__url)) as ?Document1)
    BIND (URI(CONCAT('http://purl.org/vocab/frbr/core/place/',?art-work__birthPlaceLabel)) as ?Place1)
}