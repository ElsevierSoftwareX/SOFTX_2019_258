PREFIX scharp:    <http://scharp.usc.isi.edu/ontology/>
PREFIX memex:     <http://dig.isi.edu/ontology/memex/>
PREFIX jarql:     <http://jarql.com/>

CONSTRUCT {
    ?CulturalHeritageObject1 dcterms:description ?Description.
    ?CulturalHeritageObject1 rdf:type aac-ont:CulturalHeritageObject.
    ?Concept1 skos2:prefLabel ?Heilbrunn Timeline of Art History.
    ?Concept1 rdf:type skos2:Concept.
    ?CulturalHeritageObject1 dcterms:extent ?Dimensions.
    ?CulturalHeritageObject1 rdf:type aac-ont:CulturalHeritageObject.
    ?Concept2 skos2:prefLabel ?Classification.
    ?Concept2 rdf:type skos2:Concept.
    ?CulturalHeritageObject2 aac-ont:objectId ?related-artworks.
    ?CulturalHeritageObject2 rdf:type aac-ont:CulturalHeritageObject.
    ?CulturalHeritageObject1 dcterms:medium ?Medium.
    ?CulturalHeritageObject1 rdf:type aac-ont:CulturalHeritageObject.
    ?Person1 ElementsGr2:nameOfThePerson ?Who.
    ?Person1 rdf:type aac-ont:Person.
    ?CulturalHeritageObject1 aac-ont:objectId ?id.
    ?CulturalHeritageObject1 rdf:type aac-ont:CulturalHeritageObject.
    ?Place1 rdfs:label ?Geography.
    ?Place1 rdf:type aac-ont:Place.
    ?CulturalHeritageObject1 dcterms:provenance ?Provenance.
    ?CulturalHeritageObject1 rdf:type aac-ont:CulturalHeritageObject.
    ?CulturalHeritageObject1 dcterms:provenance ?Credit Line.
    ?CulturalHeritageObject1 rdf:type aac-ont:CulturalHeritageObject.
    ?CulturalHeritageObject1 dcterms:created ?Date.
    ?CulturalHeritageObject1 rdf:type aac-ont:CulturalHeritageObject.
    ?CulturalHeritageObject1 aac-ont:accessionNumber ?Accession Number.
    ?CulturalHeritageObject1 rdf:type aac-ont:CulturalHeritageObject.
    ?WebResource1 rdfs:label ?image.
    ?WebResource1 rdf:type edm:WebResource.
    ?CulturalHeritageObject1 edm:isRelatedTo ?CulturalHeritageObject2.
    ?CulturalHeritageObject1 dcterms:creator ?Person1.
    ?CulturalHeritageObject1 aac-ont:associatedPlace ?Place1.
    ?CulturalHeritageObject1 dcterms:subject ?Concept1.
    ?CulturalHeritageObject1 edm:hasType ?Concept2.
    ?EuropeanaAggregation1 edm:aggregatedCHO ?CulturalHeritageObject1.
    ?EuropeanaAggregation1 edm:hasView ?WebResource1.
}
WHERE {
    ?root a jarql:Root.
    OPTIONAL { ?root jarql:Description ?Description. }
    OPTIONAL { ?root jarql:Heilbrunn Timeline of Art History ?Heilbrunn Timeline of Art History. }
    OPTIONAL { ?root jarql:Dimensions ?Dimensions. }
    OPTIONAL { ?root jarql:Classification ?Classification. }
    OPTIONAL { ?root jarql:related-artworks ?related-artworks. }
    OPTIONAL { ?root jarql:Medium ?Medium. }
    OPTIONAL { ?root jarql:Who ?Who. }
    OPTIONAL { ?root jarql:id ?id. }
    OPTIONAL { ?root jarql:Geography ?Geography. }
    OPTIONAL { ?root jarql:Provenance ?Provenance. }
    OPTIONAL { ?root jarql:Credit Line ?Credit Line. }
    OPTIONAL { ?root jarql:Date ?Date. }
    OPTIONAL { ?root jarql:Accession Number ?Accession Number. }
    OPTIONAL { ?root jarql:image ?image. }
    BIND (URI(CONCAT('http://www.americanartcollaborative.org/ontology//culturalheritageobject/',?Description)) as ?CulturalHeritageObject1)
    BIND (URI(CONCAT('http://purl.org/vocab/frbr/core/concept/',?Heilbrunn Timeline of Art History)) as ?Concept1)
    BIND (URI(CONCAT('http://purl.org/vocab/frbr/core/concept/',?Classification)) as ?Concept2)
    BIND (URI(CONCAT('http://www.americanartcollaborative.org/ontology//culturalheritageobject/',?related-artworks)) as ?CulturalHeritageObject2)
    BIND (URI(CONCAT('http://purl.org/vocab/frbr/core/person/',?Who)) as ?Person1)
    BIND (URI(CONCAT('http://purl.org/vocab/frbr/core/place/',?Geography)) as ?Place1)
    BIND (URI(CONCAT('http://www.europeana.eu/schemas/edm//webresource/',?image)) as ?WebResource1)
    BIND (URI(CONCAT('http://www.europeana.eu/schemas/edm//europeanaaggregation/',?Description)) as ?EuropeanaAggregation1)
}