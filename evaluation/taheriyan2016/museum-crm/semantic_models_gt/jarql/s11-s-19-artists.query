CONSTRUCT {
    ?E33_Linguistic_Object1 crm:P3_has_note ?birth_data_and_place.
    ?E33_Linguistic_Object1 rdf:type crm:E33_Linguistic_Object.
    ?E33_Linguistic_Object2 crm:P3_has_note ?long_biography.
    ?E33_Linguistic_Object2 rdf:type crm:E33_Linguistic_Object.
    ?E33_Linguistic_Object3 crm:P3_has_note ?occupation.
    ?E33_Linguistic_Object3 rdf:type crm:E33_Linguistic_Object.
    ?E74_Group1 rdfs:label ?nationality.
    ?E74_Group1 rdf:type crm:E74_Group.
    ?E82_Actor_Appellation1 rdfs:label ?artist_name.
    ?E82_Actor_Appellation1 rdf:type crm:E82_Actor_Appellation.
}
WHERE {
    ?root a jarql:Root.
    OPTIONAL { ?root jarql:birth_data_and_place ?birth_data_and_place. }
    OPTIONAL { ?root jarql:long_biography ?long_biography. }
    OPTIONAL { ?root jarql:occupation ?occupation. }
    OPTIONAL { ?root jarql:nationality ?nationality. }
    OPTIONAL { ?root jarql:artist_name ?artist_name. }
}