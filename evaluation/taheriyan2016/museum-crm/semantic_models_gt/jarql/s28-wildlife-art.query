PREFIX schema:    <http://schema.org/>
PREFIX rdfs:      <http://www.w3.org/2000/01/rdf-schema#>
PREFIX pc:        <http://purl.org/procurement/public-contracts#>
PREFIX gr:        <http://purl.org/goodrelations/v1#>
PREFIX owl:       <http://www.w3.org/2002/07/owl#>
PREFIX adms:      <http://www.w3.org/ns/adms#>
PREFIX c4n:       <http://vocab.deri.ie/c4n#>
PREFIX dcterms:   <http://purl.org/dc/terms/>
PREFIX foaf:      <http://xmlns.com/foaf/0.1/>
PREFIX loted:     <http://www.loted.eu/ontology#>
PREFIX payment:   <http://reference.data.gov.uk/def/payment#>
PREFIX qb:        <http://purl.org/linked-data/cube#>
PREFIX rdf:       <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX skos:      <http://www.w3.org/2004/02/skos/core#>
PREFIX vann:      <http://purl.org/vocab/vann/>
PREFIX xsd:       <http://www.w3.org/2001/XMLSchema#>
PREFIX conference: <http://conference#>
PREFIX cmt:       <http://cmt#>
PREFIX sigkdd:    <http://sigkdd#>
PREFIX crm:       <http://erlangen-crm.org/current/>
PREFIX id:        <http://collection.britishmuseum.org/id/>
PREFIX idThes:    <http://collection.britishmuseum.org/id/thesauri/>
PREFIX prov:      <http://www.w3.org/ns/prov#>
PREFIX bmo:       <http://collection.britishmuseum.org/id/ontology/>
PREFIX jarql:     <http://jarql.com/>

CONSTRUCT {
    ?E22_Man-Made_Object1 crm:P3_has_note ?Description.
    ?E22_Man-Made_Object1 rdf:type crm:E22_Man-Made_Object.
    ?E33_Linguistic_Object1 crm:P3_has_note ?Collector Information.
    ?E33_Linguistic_Object1 rdf:type crm:E33_Linguistic_Object.
    ?E35_Title1 rdfs:label ?Title.
    ?E35_Title1 rdf:type crm:E35_Title.
    ?E40_Legal_Body1 rdfs:label ?Owner.
    ?E40_Legal_Body1 rdf:type crm:E40_Legal_Body.
    ?E48_Place_Name1 rdfs:label ?Place Made.
    ?E48_Place_Name1 rdf:type crm:E48_Place_Name.
    ?E52_Time-Span1 crm:P82_at_some_time_within ?Maker Birth Date.
    ?E52_Time-Span1 rdf:type crm:E52_Time-Span.
    ?E52_Time-Span2 crm:P82_at_some_time_within ?Maker Death Date.
    ?E52_Time-Span2 rdf:type crm:E52_Time-Span.
    ?E54_Dimension1 crm:P3_has_note ?Measurements.
    ?E54_Dimension1 rdf:type crm:E54_Dimension.
    ?E55_Type1 rdfs:label ?Category.
    ?E55_Type1 rdf:type crm:E55_Type.
    ?E55_Type2 rdfs:label ?Materials.
    ?E55_Type2 rdf:type crm:E55_Type.
    ?E74_Group1 rdfs:label ?Nationality.
    ?E74_Group1 rdf:type crm:E74_Group.
    ?E82_Actor_Appellation1 rdfs:label ?Maker.
    ?E82_Actor_Appellation1 rdf:type crm:E82_Actor_Appellation.
    ?E8_Acquisition1 crm:P3_has_note ?Credit Line.
    ?E8_Acquisition1 rdf:type crm:E8_Acquisition.
    ?E8_Acquisition2 crm:P3_has_note ?How Acquired?.
    ?E8_Acquisition2 rdf:type crm:E8_Acquisition.
}
WHERE {
    ?root a jarql:Root.
    OPTIONAL { ?root jarql:Description ?Description. }
    OPTIONAL { ?root jarql:Collector Information ?Collector Information. }
    OPTIONAL { ?root jarql:Title ?Title. }
    OPTIONAL { ?root jarql:Owner ?Owner. }
    OPTIONAL { ?root jarql:Place Made ?Place Made. }
    OPTIONAL { ?root jarql:Maker Birth Date ?Maker Birth Date. }
    OPTIONAL { ?root jarql:Maker Death Date ?Maker Death Date. }
    OPTIONAL { ?root jarql:Measurements ?Measurements. }
    OPTIONAL { ?root jarql:Category ?Category. }
    OPTIONAL { ?root jarql:Materials ?Materials. }
    OPTIONAL { ?root jarql:Nationality ?Nationality. }
    OPTIONAL { ?root jarql:Maker ?Maker. }
    OPTIONAL { ?root jarql:Credit Line ?Credit Line. }
    OPTIONAL { ?root jarql:How Acquired? ?How Acquired?. }
}