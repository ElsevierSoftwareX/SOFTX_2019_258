PREFIX bmo:       <http://collection.britishmuseum.org/id/ontology/>
PREFIX jarql:     <http://jarql.com/>

CONSTRUCT {
    ?E22_Man-Made_Object1 crm:P3_has_note ?DESCRIPTION.
    ?E22_Man-Made_Object1 rdf:type crm:E22_Man-Made_Object.
    ?E31_Document1 http://isi.edu/integration/karma/dev#classLink ?VIDEO.
    ?E31_Document1 rdf:type crm:E31_Document.
    ?E35_Title1 rdfs:label ?TITLE NO AUTHOR.
    ?E35_Title1 rdf:type crm:E35_Title.
    ?E38_Image1 http://isi.edu/integration/karma/dev#classLink ?IMAGESRC.
    ?E38_Image1 rdf:type crm:E38_Image.
    ?E82_Actor_Appellation1 rdfs:label ?AUTHOR.
    ?E82_Actor_Appellation1 rdf:type crm:E82_Actor_Appellation.
}
WHERE {
    ?root a jarql:Root.
    OPTIONAL { ?root jarql:DESCRIPTION ?DESCRIPTION. }
    OPTIONAL { ?root jarql:VIDEO ?VIDEO. }
    OPTIONAL { ?root jarql:TITLE NO AUTHOR ?TITLE NO AUTHOR. }
    OPTIONAL { ?root jarql:IMAGESRC ?IMAGESRC. }
    OPTIONAL { ?root jarql:AUTHOR ?AUTHOR. }
}