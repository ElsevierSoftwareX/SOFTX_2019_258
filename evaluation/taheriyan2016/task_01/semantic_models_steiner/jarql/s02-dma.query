CONSTRUCT {
    ?E21_Person1 rdfs:label ?Artist URI.
    ?E21_Person1 rdf:type crm:E21_Person.
    ?E22_Man-Made_Object1 rdfs:label ?Object_URI.
    ?E22_Man-Made_Object1 rdf:type crm:E22_Man-Made_Object.
    ?E30_Right1 crm:P3_has_note ?Object Rights.
    ?E30_Right1 rdf:type crm:E30_Right.
    ?E35_Title1 rdfs:label ?Object Title.
    ?E35_Title1 rdf:type crm:E35_Title.
    ?E40_Legal_Body1 rdfs:label ?Museum URI.
    ?E40_Legal_Body1 rdf:type crm:E40_Legal_Body.
    ?E40_Legal_Body1 rdfs:label ?Object Link Source.
    ?E40_Legal_Body1 rdf:type crm:E40_Legal_Body.
    ?E52_Time-Span1 crm:P82_at_some_time_within ?Artist Birth Date.
    ?E52_Time-Span1 rdf:type crm:E52_Time-Span.
    ?E52_Time-Span2 crm:P82_at_some_time_within ?Artist Death Date.
    ?E52_Time-Span2 rdf:type crm:E52_Time-Span.
    ?E52_Time-Span3 crm:P82a_begin_of_the_begin ?Object Earliest Date.
    ?E52_Time-Span3 rdf:type crm:E52_Time-Span.
    ?E52_Time-Span3 crm:P82b_end_of_the_end ?Object Latest Date.
    ?E52_Time-Span3 rdf:type crm:E52_Time-Span.
    ?E54_Dimension1 crm:P3_has_note ?dimensions.
    ?E54_Dimension1 rdf:type crm:E54_Dimension.
    ?E55_Type1 rdfs:label ?Object Work Type URI.
    ?E55_Type1 rdf:type crm:E55_Type.
    ?E55_Type1 rdfs:label ?Object Work Type.
    ?E55_Type1 rdf:type crm:E55_Type.
    ?E55_Type2 rdfs:label ?Medium URI.
    ?E55_Type2 rdf:type crm:E55_Type.
    ?E55_Type2 rdfs:label ?Object Facet Value 1.
    ?E55_Type2 rdf:type crm:E55_Type.
    ?E74_Group1 rdfs:label ?Nationality_URI.
    ?E74_Group1 rdf:type crm:E74_Group.
    ?E74_Group1 rdfs:label ?Artist Nationality.
    ?E74_Group1 rdf:type crm:E74_Group.
    ?E82_Actor_Appellation1 rdfs:label ?Artist Appellation.
    ?E82_Actor_Appellation1 rdf:type crm:E82_Actor_Appellation.
    ?E82_Actor_Appellation1 rdfs:label ?Artist Name.
    ?E82_Actor_Appellation1 rdf:type crm:E82_Actor_Appellation.
    ?E21_Person1 bmo:PX_pupil_of ?E39_Actor1.
    ?E22_Man-Made_Object1 crm:P62_depicts ?E82_Actor_Appellation1.
    ?E30_Right1 crm:P75i_is_possessed_by ?E39_Actor1.
    ?E35_Title1 crm:P102i_is_title_of ?E71_Man-Made_Thing1.
    ?E39_Actor1 crm:P131_is_identified_by ?E82_Actor_Appellation1.
    ?E40_Legal_Body1 crm:P131_is_identified_by ?E82_Actor_Appellation1.
    ?E52_Time-Span1 crm:P83_had_at_least_duration ?E54_Dimension1.
    ?E52_Time-Span2 crm:P83_had_at_least_duration ?E54_Dimension1.
    ?E52_Time-Span3 crm:P83_had_at_least_duration ?E54_Dimension1.
    ?E54_Dimension1 crm:P43i_is_dimension_of ?E70_Thing1.
    ?E55_Type1 crm:P127_has_broader_term ?E55_Type2.
    ?E55_Type2 crm:P103i_was_intention_of ?E82_Actor_Appellation1.
    ?E70_Thing2 crm:P101_had_as_general_use ?E55_Type2.
    ?E71_Man-Made_Thing2 crm:P103_was_intended_for ?E55_Type2.
    ?E74_Group1 crm:P107_has_current_or_former_member ?E39_Actor1.
}
WHERE {
    ?root a jarql:Root.
    OPTIONAL { ?root jarql:Artist URI ?Artist URI. }
    OPTIONAL { ?root jarql:Object_URI ?Object_URI. }
    OPTIONAL { ?root jarql:Object Rights ?Object Rights. }
    OPTIONAL { ?root jarql:Object Title ?Object Title. }
    OPTIONAL { ?root jarql:Museum URI ?Museum URI. }
    OPTIONAL { ?root jarql:Object Link Source ?Object Link Source. }
    OPTIONAL { ?root jarql:Artist Birth Date ?Artist Birth Date. }
    OPTIONAL { ?root jarql:Artist Death Date ?Artist Death Date. }
    OPTIONAL { ?root jarql:Object Earliest Date ?Object Earliest Date. }
    OPTIONAL { ?root jarql:Object Latest Date ?Object Latest Date. }
    OPTIONAL { ?root jarql:dimensions ?dimensions. }
    OPTIONAL { ?root jarql:Object Work Type URI ?Object Work Type URI. }
    OPTIONAL { ?root jarql:Object Work Type ?Object Work Type. }
    OPTIONAL { ?root jarql:Medium URI ?Medium URI. }
    OPTIONAL { ?root jarql:Object Facet Value 1 ?Object Facet Value 1. }
    OPTIONAL { ?root jarql:Nationality_URI ?Nationality_URI. }
    OPTIONAL { ?root jarql:Artist Nationality ?Artist Nationality. }
    OPTIONAL { ?root jarql:Artist Appellation ?Artist Appellation. }
    OPTIONAL { ?root jarql:Artist Name ?Artist Name. }
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e21_person/',?Artist URI)) as ?E21_Person1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e22_man-made_object/',?Object_URI)) as ?E22_Man-Made_Object1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e40_legal_body/',?Museum URI)) as ?E40_Legal_Body1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e55_type/',?Object Work Type URI)) as ?E55_Type1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e55_type/',?Medium URI)) as ?E55_Type2)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e74_group/',?Nationality_URI)) as ?E74_Group1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e82_actor_appellation/',?Artist Appellation)) as ?E82_Actor_Appellation1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e39_actor/',?Artist URI)) as ?E39_Actor1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e71_man-made_thing/',?Object Title)) as ?E71_Man-Made_Thing1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e70_thing/',?dimensions)) as ?E70_Thing1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e70_thing/',?Medium URI)) as ?E70_Thing2)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e71_man-made_thing/',?Medium URI)) as ?E71_Man-Made_Thing2)
}