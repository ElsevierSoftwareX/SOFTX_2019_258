PREFIX scharp:    <http://scharp.usc.isi.edu/ontology/>
PREFIX memex:     <http://dig.isi.edu/ontology/memex/>
PREFIX dc:        <http://purl.org/dc/elements/1.1/>
PREFIX jarql:     <http://jarql.com/>

CONSTRUCT {
    ?E21_Person1 rdfs:label ?Artist_URI.
    ?E21_Person1 rdf:type crm:E21_Person.
    ?E21_Person2 crm:P3_has_note ?Description.
    ?E21_Person2 rdf:type crm:E21_Person.
    ?E22_Man-Made_Object1 rdfs:label ?Object_URI.
    ?E22_Man-Made_Object1 rdf:type crm:E22_Man-Made_Object.
    ?E38_Image1 rdfs:label ?Image.
    ?E38_Image1 rdf:type crm:E38_Image.
    ?E40_Legal_Body1 rdfs:label ?Owner.
    ?E40_Legal_Body1 rdf:type crm:E40_Legal_Body.
    ?E52_Time-Span1 crm:P82_at_some_time_within ?BornDate.
    ?E52_Time-Span1 rdf:type crm:E52_Time-Span.
    ?E52_Time-Span2 crm:P82_at_some_time_within ?deathDate.
    ?E52_Time-Span2 rdf:type crm:E52_Time-Span.
    ?E52_Time-Span3 crm:P82_at_some_time_within ?DateOfWork.
    ?E52_Time-Span3 rdf:type crm:E52_Time-Span.
    ?E52_Time-Span4 crm:P82_at_some_time_within ?AquisitionDate.
    ?E52_Time-Span4 rdf:type crm:E52_Time-Span.
    ?E54_Dimension1 crm:P3_has_note ?Dimension.
    ?E54_Dimension1 rdf:type crm:E54_Dimension.
    ?E55_Type1 rdfs:label ?Classification_URI.
    ?E55_Type1 rdf:type crm:E55_Type.
    ?E55_Type1 rdfs:label ?Classification.
    ?E55_Type1 rdf:type crm:E55_Type.
    ?E55_Type2 rdfs:label ?Medium_URI.
    ?E55_Type2 rdf:type crm:E55_Type.
    ?E55_Type2 rdfs:label ?Medium.
    ?E55_Type2 rdf:type crm:E55_Type.
    ?E55_Type3 rdfs:label ?Subclassification_URI.
    ?E55_Type3 rdf:type crm:E55_Type.
    ?E55_Type3 rdfs:label ?Subclassification.
    ?E55_Type3 rdf:type crm:E55_Type.
    ?E82_Actor_Appellation1 rdfs:label ?Artist_Appellation_URI.
    ?E82_Actor_Appellation1 rdf:type crm:E82_Actor_Appellation.
    ?E82_Actor_Appellation1 rdfs:label ?Artist.
    ?E82_Actor_Appellation1 rdf:type crm:E82_Actor_Appellation.
    ?E82_Actor_Appellation2 rdfs:label ?Sitter.
    ?E82_Actor_Appellation2 rdf:type crm:E82_Actor_Appellation.
    ?E8_Acquisition1 crm:P3_has_note ?CreditLine.
    ?E8_Acquisition1 rdf:type crm:E8_Acquisition.
    ?E18_Physical_Thing1 crm:P24i_changed_ownership_through ?E8_Acquisition1.
    ?E21_Person1 bmo:PX_pupil_of ?E39_Actor1.
    ?E21_Person2 bmo:PX_pupil_of ?E39_Actor2.
    ?E22_Man-Made_Object1 crm:P62_depicts ?E8_Acquisition1.
    ?E38_Image1 crm:P138_represents ?E8_Acquisition1.
    ?E39_Actor1 crm:P49i_is_former_or_current_keeper_of ?E18_Physical_Thing1.
    ?E40_Legal_Body1 crm:P11i_participated_in ?E8_Acquisition1.
    ?E52_Time-Span1 crm:P83_had_at_least_duration ?E54_Dimension1.
    ?E52_Time-Span2 crm:P83_had_at_least_duration ?E54_Dimension1.
    ?E52_Time-Span3 crm:P83_had_at_least_duration ?E54_Dimension1.
    ?E52_Time-Span4 crm:P83_had_at_least_duration ?E54_Dimension1.
    ?E54_Dimension1 crm:P43i_is_dimension_of ?E70_Thing1.
    ?E55_Type1 crm:P127_has_broader_term ?E55_Type3.
    ?E55_Type2 crm:P127_has_broader_term ?E55_Type3.
    ?E55_Type3 crm:P125i_was_type_of_object_used_in ?E8_Acquisition1.
    ?E70_Thing3 crm:P101_had_as_general_use ?E55_Type3.
    ?E82_Actor_Appellation1 crm:P131i_identifies ?E39_Actor1.
    ?E82_Actor_Appellation2 crm:P131i_identifies ?E39_Actor2.
}
WHERE {
    ?root a jarql:Root.
    OPTIONAL { ?root jarql:Artist_URI ?Artist_URI. }
    OPTIONAL { ?root jarql:Description ?Description. }
    OPTIONAL { ?root jarql:Object_URI ?Object_URI. }
    OPTIONAL { ?root jarql:Image ?Image. }
    OPTIONAL { ?root jarql:Owner ?Owner. }
    OPTIONAL { ?root jarql:BornDate ?BornDate. }
    OPTIONAL { ?root jarql:deathDate ?deathDate. }
    OPTIONAL { ?root jarql:DateOfWork ?DateOfWork. }
    OPTIONAL { ?root jarql:AquisitionDate ?AquisitionDate. }
    OPTIONAL { ?root jarql:Dimension ?Dimension. }
    OPTIONAL { ?root jarql:Classification_URI ?Classification_URI. }
    OPTIONAL { ?root jarql:Classification ?Classification. }
    OPTIONAL { ?root jarql:Medium_URI ?Medium_URI. }
    OPTIONAL { ?root jarql:Medium ?Medium. }
    OPTIONAL { ?root jarql:Subclassification_URI ?Subclassification_URI. }
    OPTIONAL { ?root jarql:Subclassification ?Subclassification. }
    OPTIONAL { ?root jarql:Artist_Appellation_URI ?Artist_Appellation_URI. }
    OPTIONAL { ?root jarql:Artist ?Artist. }
    OPTIONAL { ?root jarql:Sitter ?Sitter. }
    OPTIONAL { ?root jarql:CreditLine ?CreditLine. }
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e21_person/',?Artist_URI)) as ?E21_Person1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e22_man-made_object/',?Object_URI)) as ?E22_Man-Made_Object1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e38_image/',?Image)) as ?E38_Image1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e55_type/',?Classification_URI)) as ?E55_Type1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e55_type/',?Medium_URI)) as ?E55_Type2)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e55_type/',?Subclassification_URI)) as ?E55_Type3)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e82_actor_appellation/',?Artist_Appellation_URI)) as ?E82_Actor_Appellation1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e18_physical_thing/',?CreditLine)) as ?E18_Physical_Thing1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e39_actor/',?Artist_URI)) as ?E39_Actor1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e39_actor/',?Description)) as ?E39_Actor2)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e70_thing/',?Dimension)) as ?E70_Thing1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e70_thing/',?Subclassification_URI)) as ?E70_Thing3)
}