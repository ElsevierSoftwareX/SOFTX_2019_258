CONSTRUCT {
    ?E21_Person1 rdfs:label ?artist_uri.
    ?E21_Person1 rdf:type crm:E21_Person.
    ?E22_Man-Made_Object1 rdfs:label ?sourceURL.
    ?E22_Man-Made_Object1 rdf:type crm:E22_Man-Made_Object.
    ?E33_Linguistic_Object1 crm:P3_has_note ?provenance.
    ?E33_Linguistic_Object1 rdf:type crm:E33_Linguistic_Object.
    ?E33_Linguistic_Object2 crm:P3_has_note ?galleryLabel.
    ?E33_Linguistic_Object2 rdf:type crm:E33_Linguistic_Object.
    ?E35_Title1 rdfs:label ?title.
    ?E35_Title1 rdf:type crm:E35_Title.
    ?E38_Image1 rdfs:label ?imageURL.
    ?E38_Image1 rdf:type crm:E38_Image.
    ?E52_Time-Span1 crm:P82_at_some_time_within ?birthDate.
    ?E52_Time-Span1 rdf:type crm:E52_Time-Span.
    ?E52_Time-Span2 crm:P82_at_some_time_within ?deathDate.
    ?E52_Time-Span2 rdf:type crm:E52_Time-Span.
    ?E52_Time-Span4 crm:P82_at_some_time_within ?creationDate.
    ?E52_Time-Span4 rdf:type crm:E52_Time-Span.
    ?E54_Dimension1 crm:P3_has_note ?dimensions.
    ?E54_Dimension1 rdf:type crm:E54_Dimension.
    ?E55_Type1 rdfs:label ?material_URI.
    ?E55_Type1 rdf:type crm:E55_Type.
    ?E55_Type1 rdfs:label ?materials.
    ?E55_Type1 rdf:type crm:E55_Type.
    ?E55_Type2 rdfs:label ?provenance_type_uri.
    ?E55_Type2 rdf:type crm:E55_Type.
    ?E55_Type3 rdfs:label ?galleryLabel_type_uri.
    ?E55_Type3 rdf:type crm:E55_Type.
    ?E74_Group1 rdfs:label ?nationality_URI.
    ?E74_Group1 rdf:type crm:E74_Group.
    ?E74_Group1 rdfs:label ?nationality.
    ?E74_Group1 rdf:type crm:E74_Group.
    ?E82_Actor_Appellation1 rdfs:label ?artist_appellation_uri.
    ?E82_Actor_Appellation1 rdf:type crm:E82_Actor_Appellation.
    ?E82_Actor_Appellation1 rdfs:label ?artist.
    ?E82_Actor_Appellation1 rdf:type crm:E82_Actor_Appellation.
    ?E8_Acquisition1 crm:P3_has_note ?creditLine.
    ?E8_Acquisition1 rdf:type crm:E8_Acquisition.
    ?E18_Physical_Thing1 crm:P24i_changed_ownership_through ?E8_Acquisition1.
    ?E21_Person1 bmo:PX_pupil_of ?E39_Actor1.
    ?E22_Man-Made_Object1 crm:P62_depicts ?E8_Acquisition1.
    ?E33_Linguistic_Object1 crm:P73_has_translation ?E33_Linguistic_Object2.
    ?E33_Linguistic_Object2 crm:P129_is_about ?E8_Acquisition1.
    ?E35_Title1 crm:P102i_is_title_of ?E71_Man-Made_Thing1.
    ?E38_Image1 crm:P138_represents ?E8_Acquisition1.
    ?E39_Actor1 crm:P49i_is_former_or_current_keeper_of ?E18_Physical_Thing1.
    ?E39_Actor1 crm:P131_is_identified_by ?E82_Actor_Appellation1.
    ?E52_Time-Span1 crm:P83_had_at_least_duration ?E54_Dimension1.
    ?E52_Time-Span2 crm:P83_had_at_least_duration ?E54_Dimension1.
    ?E52_Time-Span4 crm:P83_had_at_least_duration ?E54_Dimension1.
    ?E54_Dimension1 crm:P43i_is_dimension_of ?E70_Thing1.
    ?E55_Type1 crm:P127_has_broader_term ?E55_Type3.
    ?E55_Type2 crm:P127_has_broader_term ?E55_Type3.
    ?E55_Type3 crm:P125i_was_type_of_object_used_in ?E8_Acquisition1.
    ?E70_Thing3 crm:P101_had_as_general_use ?E55_Type3.
    ?E71_Man-Made_Thing3 crm:P103_was_intended_for ?E55_Type3.
    ?E74_Group1 crm:P107_has_current_or_former_member ?E39_Actor1.
}
WHERE {
    ?root a jarql:Root.
    OPTIONAL { ?root jarql:artist_uri ?artist_uri. }
    OPTIONAL { ?root jarql:sourceURL ?sourceURL. }
    OPTIONAL { ?root jarql:provenance ?provenance. }
    OPTIONAL { ?root jarql:galleryLabel ?galleryLabel. }
    OPTIONAL { ?root jarql:title ?title. }
    OPTIONAL { ?root jarql:imageURL ?imageURL. }
    OPTIONAL { ?root jarql:birthDate ?birthDate. }
    OPTIONAL { ?root jarql:deathDate ?deathDate. }
    OPTIONAL { ?root jarql:creationDate ?creationDate. }
    OPTIONAL { ?root jarql:dimensions ?dimensions. }
    OPTIONAL { ?root jarql:material_URI ?material_URI. }
    OPTIONAL { ?root jarql:materials ?materials. }
    OPTIONAL { ?root jarql:provenance_type_uri ?provenance_type_uri. }
    OPTIONAL { ?root jarql:galleryLabel_type_uri ?galleryLabel_type_uri. }
    OPTIONAL { ?root jarql:nationality_URI ?nationality_URI. }
    OPTIONAL { ?root jarql:nationality ?nationality. }
    OPTIONAL { ?root jarql:artist_appellation_uri ?artist_appellation_uri. }
    OPTIONAL { ?root jarql:artist ?artist. }
    OPTIONAL { ?root jarql:creditLine ?creditLine. }
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e21_person/',?artist_uri)) as ?E21_Person1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e22_man-made_object/',?sourceURL)) as ?E22_Man-Made_Object1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e38_image/',?imageURL)) as ?E38_Image1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e55_type/',?material_URI)) as ?E55_Type1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e55_type/',?provenance_type_uri)) as ?E55_Type2)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e55_type/',?galleryLabel_type_uri)) as ?E55_Type3)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e74_group/',?nationality_URI)) as ?E74_Group1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e82_actor_appellation/',?artist_appellation_uri)) as ?E82_Actor_Appellation1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e18_physical_thing/',?creditLine)) as ?E18_Physical_Thing1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e39_actor/',?artist_uri)) as ?E39_Actor1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e71_man-made_thing/',?title)) as ?E71_Man-Made_Thing1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e70_thing/',?dimensions)) as ?E70_Thing1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e70_thing/',?galleryLabel_type_uri)) as ?E70_Thing3)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e71_man-made_thing/',?galleryLabel_type_uri)) as ?E71_Man-Made_Thing3)
}