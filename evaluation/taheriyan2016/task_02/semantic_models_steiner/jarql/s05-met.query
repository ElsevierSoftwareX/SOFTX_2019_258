CONSTRUCT {
    ?E21_Person1 rdfs:label ?Person URI.
    ?E21_Person1 rdf:type crm:E21_Person.
    ?E22_Man-Made_Object1 crm:P3_has_note ?Description.
    ?E22_Man-Made_Object1 rdf:type crm:E22_Man-Made_Object.
    ?E22_Man-Made_Object1 rdfs:label ?Object URI.
    ?E22_Man-Made_Object1 rdf:type crm:E22_Man-Made_Object.
    ?E33_Linguistic_Object1 crm:P3_has_note ?What.
    ?E33_Linguistic_Object1 rdf:type crm:E33_Linguistic_Object.
    ?E38_Image1 rdfs:label ?image.
    ?E38_Image1 rdf:type crm:E38_Image.
    ?E44_Place_Appellation1 rdfs:label ?Geography.
    ?E44_Place_Appellation1 rdf:type crm:E44_Place_Appellation.
    ?E52_Time-Span1 crm:P82_at_some_time_within ?Date.
    ?E52_Time-Span1 rdf:type crm:E52_Time-Span.
    ?E54_Dimension1 crm:P3_has_note ?Dimensions.
    ?E54_Dimension1 rdf:type crm:E54_Dimension.
    ?E55_Type1 rdfs:label ?Classification URI.
    ?E55_Type1 rdf:type crm:E55_Type.
    ?E55_Type1 rdfs:label ?Classification.
    ?E55_Type1 rdf:type crm:E55_Type.
    ?E55_Type2 rdfs:label ?Culture URI.
    ?E55_Type2 rdf:type crm:E55_Type.
    ?E55_Type2 rdfs:label ?Culture.
    ?E55_Type2 rdf:type crm:E55_Type.
    ?E55_Type3 rdfs:label ?Medium URI.
    ?E55_Type3 rdf:type crm:E55_Type.
    ?E55_Type3 rdfs:label ?Medium.
    ?E55_Type3 rdf:type crm:E55_Type.
    ?E55_Type4 rdfs:label ?ProvenanceTypeURI.
    ?E55_Type4 rdf:type crm:E55_Type.
    ?E82_Actor_Appellation1 rdfs:label ?Appellation URI.
    ?E82_Actor_Appellation1 rdf:type crm:E82_Actor_Appellation.
    ?E82_Actor_Appellation1 rdfs:label ?Who.
    ?E82_Actor_Appellation1 rdf:type crm:E82_Actor_Appellation.
    ?E8_Acquisition1 crm:P3_has_note ?Credit Line.
    ?E8_Acquisition1 rdf:type crm:E8_Acquisition.
    ?E18_Physical_Thing1 crm:P24i_changed_ownership_through ?E8_Acquisition1.
    ?E21_Person1 bmo:PX_pupil_of ?E39_Actor1.
    ?E22_Man-Made_Object1 crm:P62_depicts ?E8_Acquisition1.
    ?E33_Linguistic_Object1 crm:P129_is_about ?E8_Acquisition1.
    ?E38_Image1 crm:P138_represents ?E8_Acquisition1.
    ?E39_Actor1 crm:P49i_is_former_or_current_keeper_of ?E18_Physical_Thing1.
    ?E44_Place_Appellation1 crm:P87i_identifies ?E53_Place1.
    ?E52_Time-Span1 crm:P83_had_at_least_duration ?E54_Dimension1.
    ?E53_Place1 crm:P53i_is_former_or_current_location_of ?E18_Physical_Thing1.
    ?E54_Dimension1 crm:P43i_is_dimension_of ?E70_Thing1.
    ?E55_Type1 crm:P127_has_broader_term ?E55_Type4.
    ?E55_Type2 crm:P127_has_broader_term ?E55_Type4.
    ?E55_Type3 crm:P127_has_broader_term ?E55_Type4.
    ?E55_Type4 crm:P125i_was_type_of_object_used_in ?E8_Acquisition1.
    ?E70_Thing4 crm:P101_had_as_general_use ?E55_Type4.
    ?E82_Actor_Appellation1 crm:P131i_identifies ?E39_Actor1.
}
WHERE {
    ?root a jarql:Root.
    OPTIONAL { ?root jarql:Person URI ?Person URI. }
    OPTIONAL { ?root jarql:Description ?Description. }
    OPTIONAL { ?root jarql:Object URI ?Object URI. }
    OPTIONAL { ?root jarql:What ?What. }
    OPTIONAL { ?root jarql:image ?image. }
    OPTIONAL { ?root jarql:Geography ?Geography. }
    OPTIONAL { ?root jarql:Date ?Date. }
    OPTIONAL { ?root jarql:Dimensions ?Dimensions. }
    OPTIONAL { ?root jarql:Classification URI ?Classification URI. }
    OPTIONAL { ?root jarql:Classification ?Classification. }
    OPTIONAL { ?root jarql:Culture URI ?Culture URI. }
    OPTIONAL { ?root jarql:Culture ?Culture. }
    OPTIONAL { ?root jarql:Medium URI ?Medium URI. }
    OPTIONAL { ?root jarql:Medium ?Medium. }
    OPTIONAL { ?root jarql:ProvenanceTypeURI ?ProvenanceTypeURI. }
    OPTIONAL { ?root jarql:Appellation URI ?Appellation URI. }
    OPTIONAL { ?root jarql:Who ?Who. }
    OPTIONAL { ?root jarql:Credit Line ?Credit Line. }
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e21_person/',?Person URI)) as ?E21_Person1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e22_man-made_object/',?Object URI)) as ?E22_Man-Made_Object1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e38_image/',?image)) as ?E38_Image1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e55_type/',?Classification URI)) as ?E55_Type1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e55_type/',?Culture URI)) as ?E55_Type2)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e55_type/',?Medium URI)) as ?E55_Type3)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e55_type/',?ProvenanceTypeURI)) as ?E55_Type4)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e82_actor_appellation/',?Appellation URI)) as ?E82_Actor_Appellation1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e18_physical_thing/',?Credit Line)) as ?E18_Physical_Thing1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e39_actor/',?Person URI)) as ?E39_Actor1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e53_place/',?Geography)) as ?E53_Place1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e70_thing/',?Dimensions)) as ?E70_Thing1)
    BIND (URI(CONCAT('http://erlangen-crm.org/current//e70_thing/',?ProvenanceTypeURI)) as ?E70_Thing4)
}