name=Q42 (Papers <-> Co-Authors)
orderNum=470

sql=SELECT COUNT(distinct "co_write_paper"."paper") \n\
    FROM "papers" \n\
    join "co_write_paper" on "papers"."id" = "co_write_paper"."paper" \n\
    join "persons" on "co_write_paper"."co_author" = "persons"."id" \n\

sparql=prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> \n\
       prefix : <http://cmt#> \n\
       SELECT (COUNT (DISTINCT ?paper) as ?cnt)  \n\
       WHERE { \n\
       ?paper :hasCo_author ?author }

categories=link,path-X,path-2
