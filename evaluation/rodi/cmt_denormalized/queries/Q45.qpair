name=Q45 (Reviewers <-> Reviews)
orderNum=500

sql=SELECT COUNT(distinct "Review"."writtenBy") \n\
FROM "Review" \n\
	JOIN "Person" ON "Review"."writtenBy" = "Person"."ID"

sparql=prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> \n\
prefix : <http://cmt#> \n\
SELECT (COUNT( DISTINCT ?review) AS ?cnt) \n\
	WHERE { \n\
	?reviewer :writeReview ?review}

